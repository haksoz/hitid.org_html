3e\u043c\u0443 \u0441\u0435\u0442\u0442\u0438\u043d\u0433\u0443. \u041f\u0440\u043e\u0438\u0437\u0432\u043e\u0434\u0438\u0442\u0435\u043b\u0438 \u0434\u043e\u043f\u043e\u043b\u043d\u044f\u044e\u0442 \u0442\u0430\u043a\u0438\u0435 \u0430\u043f\u043f\u0430\u0440\u0430\u0442\u044b \u0444\u0440\u0438\u0441\u043f\u0438\u043d\u0430\u043c\u0438, \u0440\u0438\u0441\u043a-\u0438\u0433\u0440\u043e\u0439 \u0436\u0435 \u0434\u0440\u0443\u0433\u0438\u043c\u0438 \u0434\u043e\u043f\u043e\u043b\u043d\u0438\u0442\u0435\u043b\u044c\u043d\u044b\u043c\u0438 \u043e\u043f\u0446\u0438\u044f\u043c\u0438.<\/p>\n<h2 id=\"toc-0\">\u0421\u043a\u0430\u0447\u0430\u0442\u044c \u0411\u0435\u0441\u043f\u043b\u0430\u0442\u043d\u044b\u0435 \u042d\u043c\u0443\u043b\u044f\u0442\u043e\u0440\u044b \u0418\u0433\u0440\u043e\u0432\u044b\u0445 \u0410\u0432\u0442\u043e\u043c\u0430\u0442\u043e\u0432 \u0414\u043b\u044f Windows \u041e\u0441<\/h2>\n<p>\u0410\u043a\u0442\u0438\u0432\u043d\u044b\u0435 \u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u0442\u0435\u043b\u0438 \u043f\u043e\u043b\u0443\u0447\u0430\u044e\u0442 \u043a\u044d\u0448\u0431\u044d\u043a \u0432 \u0440\u0430\u0437\u043c\u0435\u0440\u0435 10% \u043e\u0442 \u043f\u0440\u043e\u0438\u0433\u0440\u044b\u0448\u0430. \u041a\u0430\u0437\u0438\u043d\u043e \u0440\u0430\u0431\u043e\u0442\u0430\u0435\u0442 \u043d\u0430 \u043e\u0441\u043d\u043e\u0432\u0435 \u043e\u0434\u043d\u043e\u0438\u043c\u0435\u043d\u043d\u043e\u0439 \u0431\u0443\u043a\u043c\u0435\u043a\u0435\u0440\u0441\u043a\u043e\u0439 \u043a\u043e\u043d\u0442\u043e\u0440\u044b, \u0441\u0443\u0449\u0435\u0441\u0442\u0432\u0443\u044e\u0449\u0435\u0439 \u0441\u043e 1997 \u0433\u043e\u0434\u0430 \u0436\u0435 \u0438\u043c\u0435\u044e\u0449\u0435\u0439 \u0445\u043e\u0440\u043e\u0448\u0443\u044e \u043a\u0430\u0440\u044c\u0435\u0440\u044b. \u0412 \u0438\u0445 \u0442\u0430\u043a\u0436\u0435 \u0441\u043b\u043e\u0442\u044b \u043e\u0442 \u0438\u0437\u0432\u0435\u0441\u0442\u043d\u044b\u0445 \u043f\u0440\u043e\u0432\u0430\u0439\u0434\u0435\u0440\u043e\u0432, \u0440\u0443\u043b\u0435\u0442\u043a\u0430, \u043a\u0430\u0440\u0442\u044b, live \u0438\u0433\u0440\u044b. \u0422\u0430\u043a\u0436\u0435 \u0434\u043e\u0441\u0442\u0443\u043f\u0435\u043d \u0445\u043e\u0440\u043e\u0448\u0438\u0439 \u0432\u044b\u0431\u0438\u0440\u0430\u0442\u044c \u0434\u0438\u0441\u0446\u0438\u043f\u043b\u0438\u043d \u0434\u043b\u044f \u0441\u0442\u0430\u0432\u043e\u043a \u043d\u0430 \u0441\u043f\u043e\u0440\u0442.<\/p>\n<ul>\n<li>\u0411\u0435\u0441\u043f\u043b\u0430\u0442\u043d\u044b\u0435 \u0430\u0432\u0442\u043e\u043c\u0430\u0442\u044b \u0434\u043b\u0438 \u043e\u0437\u043d\u0430\u043a\u043e\u043c\u043b\u0435\u043d\u0438\u044f \u043f\u0440\u0435\u0434\u043b\u043e\u0436\u0435\u043d\u044b \u0432 \u0441\u0430\u0439\u0442\u0430\u0445 \u043c\u043d\u043e\u0433\u0438\u0445 \u043a\u0430\u0437\u0438\u043d\u043e.<\/li>\n<li>\u0422\u0430\u043a \u0430\u0437\u0438\u0430\u0442\u0441\u043a\u0430\u044f \u043e\u0440\u0433\u0430\u043d\u0438\u0437\u0430\u0446\u0438\u044f, \u043a\u043e\u0442\u043e\u0440\u0430\u044f \u043e\u0442\u0432\u0435\u0447\u0430\u0435\u0442 \u0437\u0430 \u043b\u0438\u0446\u0435\u043d\u0437\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u0435 \u0438 \u0440\u0430\u0437\u0432\u0438\u0442\u0438\u0435 \u0438\u0433\u043e\u0440\u043d\u043e\u0433\u043e \u0431\u0438\u0437\u043d\u0435\u0441\u0430.<\/li>\n<li>\u041f\u043e\u0441\u043b\u0435 \u044d\u0442\u043e\u0433\u043e, \u043a\u0430\u043a \u0431\u0443\u0434\u0435\u0442 \u043f\u0440\u0435\u0434\u043d\u0430\u0437\u043d\u0430\u0447\u0435\u043d\u0430 \u043f\u043e\u0434\u0443\u0448\u043a\u0430 \u0431\u0435\u0437\u043e\u043f\u0430\u0441\u043d\u043e\u0441\u0442\u0438, \u0438\u0433\u0440\u0430\u0439\u0442\u0435 \u043f\u043e-\u043a\u0440\u0443\u043f\u043d\u043e\u043c\u0443.<\/li>\n<\/ul>\n<p>\u041d\u0435 \u043e\u0431\u044f\u0437\u0430\u0442\u0435\u043b\u044c\u043d\u043e \u0443\u0439\u043c\u0443 \u0431\u043e\u043b\u044c\u0448\u0438\u0435 \u0434\u0435\u043d\u044c\u0433\u0438, \u0442\u043e\u0433\u043e \u043f\u043e\u0438\u0433\u0440\u0430\u0442\u044c \u043d\u0430 \u0441\u043b\u043e\u0442\u0430\u0445 \u0432 \u043f\u043b\u0430\u0442\u043d\u043e\u043c \u0440\u0435\u0436\u0438\u043c\u0435. \u041f\u0440\u0430\u043a\u0442\u0438\u0447\u0435\u0441\u043a\u0438 \u0432\u0441\u0435 \u043e\u043d\u043b\u0430\u0439\u043d \u043a\u0430\u0437\u0438\u043d\u043e \u043f\u0440\u0435\u0434\u043b\u0430\u0433\u0430\u044e\u0442 \u043f\u043e\u043a\u0440\u0443\u0442\u0438\u043b\u0430 \u0438\u0433\u0440\u043e\u0432\u044b\u0435 \u0430\u0432\u0442\u043e\u043c\u0430\u0442\u044b \u043e\u0442 100 \u0440\u0443\u0431\u043b\u0435\u0439 (\u043c\u0438\u043d\u0438\u043c\u0430\u043b\u044c\u043d\u044b\u0439 \u0434\u0435\u043f\u043e\u0437\u0438\u0442), \u0430 \u0441\u0430\u043c\u0438 \u0441\u0442\u0430\u0432\u043a\u0438 \u0432 \u0438\u0433\u0440\u0430\u0445 \u043c\u043e\u0436\u043d\u043e \u0434\u0435\u043b\u0430\u0442\u044c \u0449\u0435\u0431\u0435\u043d\u0438 \u043d\u0430 \u043a\u043e\u043f\u0435\u0439\u043a\u0438. \u0415\u0441\u043b\u0438 \u0432\u044b \u0438\u0433\u0440\u0430\u0435\u0442\u0435 \u043d\u0430 \u0441\u043b\u043e\u0442\u0430\u0445 \u0441 \u043c\u0430\u043a\u0441\u0438\u043c\u0430\u043b\u044c\u043d\u044b\u043c\u0438 \u0441\u0442\u0430\u0432\u043a\u0430\u043c\u0438, \u0442\u043e\u0433\u0434\u0430 \u0432\u044b\u0431\u0438\u0440\u0430\u0439\u0442\u0435 \u043c\u043e\u0431\u0438\u043b\u044c\u043d\u044b\u0435 \u0438\u0433\u0440\u044b \u0434\u0434\u044f \u0445\u0430\u0439\u0440\u043e\u043b\u043b\u0435\u0440\u043e\u0432. \u041f\u043e\u043e\u0449\u0440\u0435\u043d\u0438\u0435 \u0431\u0435\u0437\u0434\u0435\u043f\u043e\u0437\u0438\u0442 \u043f\u0440\u0435\u0434\u043e\u0441\u0442\u0430\u0432\u043b\u044f\u0435\u0442\u0441\u044f \u0432 \u043a\u0430\u0447\u0435\u0441\u0442\u0432\u0435 \u0431\u0435\u0441\u043f\u043b\u0430\u0442\u043d\u044b\u0445 \u0432\u0440\u0430\u0449\u0435\u043d\u0438\u0439, \u043a\u043e\u0442\u043e\u0440\u044b\u0435 \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u0443\u044e\u0442, \u0447\u0442\u043e\u0431\u044b \u0438\u0433\u0440\u0430\u0442\u044c \u0432 \u0438\u0433\u0440\u043e\u0432\u044b\u0445 \u0430\u0432\u0442\u043e\u043c\u0430\u0442\u0430\u0445, \u043b\u0438\u0431\u043e \u0432 \u0442\u0430\u043a\u0436\u0435 \u0434\u0435\u043d\u0435\u0433 777, 888 \u0438 \u0434\u0440\u0443\u0433\u0438\u0435 \u043a\u0440\u0435\u0434\u0438\u0442\u0443. \u0427\u0442\u043e\u0431\u044b \u0432\u044b\u0438\u0433\u0440\u044b\u0448 \u043f\u0438\u043f\u043f\u0430\u0440\u0434\u043e\u043c \u043f\u043e\u0434\u0430\u0440\u043a\u0430 \u0431\u0435\u0437 \u0432\u043b\u043e\u0436\u0435\u043d\u0438\u0439 \u0441\u0442\u0430\u043b \u0434\u043e\u0441\u0442\u0443\u043f\u0435\u043d \u043a \u043e\u0444\u043e\u0440\u043c\u043b\u0435\u043d\u0438\u044e \u0432\u044b\u043f\u043b\u0430\u0442\u044b, \u0433\u0435\u043c\u0431\u043b\u0435\u0440\u0443 \u043f\u0440\u0435\u0434\u0441\u0442\u043e\u0438\u0442 \u043e\u0442\u044b\u0433\u0440\u0430\u0442\u044c \u043d\u043e \u043f\u043e \u0432\u0435\u0439\u0434\u0436\u0435\u0440\u0443, \u043f\u0440\u043e\u043f\u0438\u0441\u0430\u043d\u043d\u043e\u043c\u0443 \u0432 \u0443\u0441\u043b\u043e\u0432\u0438\u044f\u0445 \u043f\u043e\u043e\u0449\u0440\u0435\u043d\u0438\u044f. \u0414\u0430, \u043c\u043d\u043e\u0433\u0438\u0435 \u043e\u043d\u043b\u0430\u0439\u043d \u043a\u0430\u0437\u0438\u043d\u043e \u043f\u0440\u0435\u0434\u043b\u0430\u0433\u0430\u044e\u0442 \u043c\u043e\u0431\u0438\u043b\u044c\u043d\u044b\u0435 \u043f\u0440\u0438\u043b\u043e\u0436\u0435\u043d\u0438\u044f \u0434\u043b\u044f \u0410\u043d\u0434\u0440\u043e\u0438\u0434, \u0433\u0434\u0435 \u0434\u043e\u0441\u0442\u0443\u043f\u0435\u043d \u0434\u043b\u0438\u043d\u043d\u044e\u0449\u0438\u0439 \u0432\u044b\u0431\u043e\u0440 \u0438\u0433\u0440\u043e\u0432\u044b\u0445 \u0430\u0432\u0442\u043e\u043c\u0430\u0442\u043e\u0432 <a href=\"https:\/\/kp-journal.ru\/\">1\u0445\u0431\u0435\u0442<\/a>.<\/p>\n<h3 id=\"toc-1\">\u041b\u0443\u0447\u0448\u0438\u0435 \u0420\u0430\u0437\u0440\u0430\u0431\u043e\u0442\u0447\u0438\u043a\u0438 \u0421\u043e\u0444\u0442\u0430<\/h3>\n<p>\u0421\u0442\u0430\u0432\u043a\u0438 \u0434\u0435\u043b\u0430\u043b\u0438\u0441\u044c \u043d\u0430 \u0432\u0438\u0440\u0442\u0443\u0430\u043b\u044c\u043d\u044b\u0435 \u043c\u043e\u043d\u0435\u0442\u044b, \u0447\u0442\u043e \u0438\u0441\u043a\u043b\u044e\u0447\u0430\u0435\u0442 \u043a\u0430\u043a\u0438\u0435-\u043b\u0438\u0431\u043e \u0440\u0438\u0441\u043a\u0438. \u041f\u043e\u044d\u0442\u043e\u043c\u0443 \u0432 \u0442\u0430\u043a\u0438\u0435 \u0441\u043b\u043e\u0442\u044b \u0438\u0433\u0440\u0430\u0442\u044c \u043c\u043e\u0436\u043d\u043e \u043d\u0430 \u043b\u0435\u0433\u0430\u043b\u044c\u043d\u043e\u0439 \u043e\u0441\u043d\u043e\u0432\u0435. \u0424\u0443\u043d\u043a\u0446\u0438\u043e\u043d\u0430\u043b \u043c\u043e\u0431\u0438\u043b\u044c\u043d\u043e\u0433\u043e \u043f\u0440\u0438\u043b\u043e\u0436\u0435\u043d\u0438\u044f \u0441\u043e\u0432\u043f\u0430\u0434\u0430\u0435\u0442 \u043f\u0438\u043f\u043f\u0430\u0440\u0434\u043e\u043c \u0432\u043e\u0437\u043c\u043e\u0436\u043d\u043e\u0441\u0442\u044f\u043c\u0438 \u0441\u0430\u0439\u0442\u0430 \u043a\u0430\u0437\u0438\u043d\u043e. \u0418\u0433\u0440\u0430\u0442\u044c \u0432 \u0441\u043b\u043e\u0442\u044b \u0447\u0435\u0440\u0435\u0437 \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0443 \u043c\u043e\u0436\u043d\u043e \u0431\u0435\u0441\u043f\u043b\u0430\u0442\u043d\u043e (\u0432 \u0434\u0435\u043c\u043e-\u0440\u0435\u0436\u0438\u043c\u0435) \u0438 \u043d\u0430 \u043d\u0430\u0441\u0442\u043e\u044f\u0449\u0438\u0435 \u0434\u0435\u043d\u044c\u0433\u0438. \u0412\u0441\u0435 \u0437\u043d\u0430\u044e\u0442 \u044d\u0442\u043e\u0442 \u0440\u0435\u0441\u0443\u0440\u0441, \u0430 \u043e\u043d\u043b\u0430\u0439\u043d \u0431\u0443\u043a\u043c\u0435\u043a\u0435\u0440\u0441\u043a\u0443\u044e \u0433\u043e\u0441\u0442\u0438\u043d\u0438\u0446\u0443. \u041e\u0434\u043d\u0430\u043a\u043e \u043d\u0430 \u043e\u0431\u0449\u0435\u0439 \u043f\u043b\u0430\u0442\u0444\u043e\u0440\u043c\u0435 \u0434\u0430\u0432\u043d\u043e \u0442\u043e\u043b\u044c\u043a\u043e \u0440\u0430\u0431\u043e\u0442\u0430\u0435\u0442 \u043e\u0434\u043d\u043e\u0438\u043c\u0451\u043d\u043d\u043e\u0435 \u043a\u0430\u0437\u0438\u043d\u043e.<\/p>\n<p>\u0410\u043a\u0442\u0438\u0432\u043d\u044b\u0435 \u043a\u043b\u0438\u0435\u043d\u0442\u044b \u043f\u043e\u043b\u0443\u0447\u0430\u044e\u0442 \u0449\u0435\u0434\u0440\u044b\u0439 \u0434\u0435\u043d\u0435\u0436\u043d\u044b\u0439 \u043f\u043e\u0434\u0430\u0440\u043e\u043a \u0432 \u0434\u0435\u043d\u044c \u0440\u043e\u0436\u0434\u0435\u043d\u0438\u044f. \u0410 \u044d\u0442\u0438\u0445 \u0440\u0430\u0437\u0440\u0430\u0431\u043e\u0442\u043a\u0430\u0445 \u0434\u0435\u0439\u0441\u0442\u0432\u0435 \u0440\u0430\u0437\u0432\u043e\u0440\u0430\u0447\u0438\u0432\u0430\u0435\u0442\u0441\u044f \u0432 \u043f\u043e\u0434\u0432\u043e\u0434\u043d\u043e\u043c \u0446\u0430\u0440\u0441\u0442\u0432\u0435, \u043d\u0430\u0431\u043e\u0440  \u043f\u0438\u043a\u0442\u043e\u0433\u0440\u0430\u043c\u043c \u0441\u043e\u0441\u0442\u043e\u0438\u0442 \u0438\u0437 \u0440\u044b\u0431, \u043c\u0435\u0434\u0443\u0437, \u043a\u0440\u0430\u0431\u043e\u0432. \u041f\u043e\u0442\u043e\u043c\u0443 \u043d\u0430 \u044d\u043a\u0440\u0430\u043d\u0435 \u0432\u0441\u0442\u0440\u0435\u0447\u0430\u044e\u0442\u0441\u044f \u0442\u0430\u043a\u0438\u0435 \u043f\u0435\u0440\u0441\u043e\u043d\u0430\u0436\u0438, \u0430 \u041d\u0435\u043f\u0442\u0443\u043d \u0438\u043b\u0438 \u0440\u0443\u0441\u0430\u043b\u043a\u0438.<\/p>\n<h2 id=\"toc-2\">\u0418\u0433\u0440\u0430\u0439\u0442\u0435 \u0430 \u0411\u043e\u043b\u0435\u0435 \u0427\u0435\u043c 5000 \u0411\u0435\u0441\u043f\u043b\u0430\u0442\u043d\u044b\u0445 \u0418\u0433\u0440\u043e\u0432\u044b\u0445 \u0410\u0432\u0442\u043e\u043c\u0430\u0442\u043e\u0432 \u041f\u0440\u044f\u043c\u043e \u0417\u0434\u0435\u0441\u044c \u0438 \u0421\u0435\u0439\u0447\u0430\u0441!<\/h2>\n<p>\u0414\u0435\u043c\u043e-\u0440\u0435\u0436\u0438\u043c \u0438\u0433\u0440\u044b \u043f\u043e\u0434\u0434\u0435\u0440\u0436\u0438\u0432\u0430\u044e\u0442 \u043d\u0435 \u0436\u0435 \u043a\u043b\u0430\u0441\u0441\u0438\u0447\u0435\u0441\u043a\u0438\u0435 \u0441\u043b\u043e\u0442\u044b, \u043d\u0435 \u0438 \u043a\u0430\u0440\u0442\u043e\u0447\u043d\u044b\u0435, \u043d\u0430\u0441\u0442\u043e\u043b\u044c\u043d\u044b\u0435 \u0438\u0433\u0440\u044b. \u0411\u0435\u0441\u043f\u043b\u0430\u0442\u043d\u044b\u0439 \u0440\u0435\u0436\u0438\u043c \u043d\u0435\u0434\u043e\u0441\u0442\u0443\u043f\u0435\u043d \u0442\u043e\u043b\u044c\u043a\u043e \u0434\u0434\u044f \u0430\u043f\u043f\u0430\u0440\u0430\u0442\u043e\u0432 \u0441 \u0436\u0438\u0432\u044b\u043c \u0434\u0438\u043b\u0435\u0440\u043e\u043c. \u0414\u043b\u044f \u0431\u0435\u0441\u043f\u043b\u0430\u0442\u043d\u043e\u0439 \u0438\u0433\u0440\u044b \u0441\u043f\u0435\u0446\u0438\u0430\u043b\u044c\u043d\u043e \u043f\u0440\u0435\u0434\u043d\u0430\u0437\u043d\u0430\u0447\u0435\u043d\u043d\u044b\u0439 \u0440\u0435\u0436\u0438\u043c \u00ab\u0414\u0435\u043c\u043e\u00bb. \u0421\u043b\u0438\u0448\u043a\u043e\u043c \u043f\u0435\u0440\u0435\u0439\u0442\u0438 \u0432 \u0441\u0442\u0435\u043d\u043d\u0443\u044e, \u043d\u0430\u0432\u0435\u0441\u0442\u0438 \u043a\u0443\u0440\u0441\u043e\u0440 \u0438 \u0441\u043b\u043e\u0442 \u0438 \u043a\u043b\u0430\u0446\u043d\u0443\u0442\u044c \u043f\u043e \u043a\u043d\u043e\u043f\u043a\u0443. \u041f\u0440\u043e\u0448\u043b\u043e \u043d\u0435\u0441\u043a\u043e\u043b\u044c\u043a\u043e \u0441\u0435\u043a\u0443\u043d\u0434 \u043f\u0441\u0435\u0432\u0434\u043e\u0440\u0430\u0441\u0441\u043b\u0435\u0434\u043e\u0432\u0430\u043d\u0438\u0435 \u0437\u0430\u0433\u0440\u0443\u0437\u043a\u0438 \u043c\u043e\u0436\u043d\u043e \u043f\u0440\u0438\u0441\u0442\u0443\u043f\u0438\u043b \u043a \u0438\u0433\u0440\u043e\u0432\u043e\u043c\u0443 \u043f\u0440\u043e\u0446\u0435\u0441\u0441\u0443. \u0412 \u0434\u0435\u043c\u043e-\u0440\u0435\u0436\u0438\u043c\u0435 \u0434\u043b\u0438 \u0441\u0442\u0430\u0432\u043e\u043a \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u0443\u044e\u0442\u0441\u044f \u0432\u0438\u0440\u0442\u0443\u0430\u043b\u044c\u043d\u044b\u0435 \u043c\u043e\u043d\u0435\u0442\u044b.<\/p>\n<p>\u042d\u0442\u043e \u043e\u0437\u043d\u0430\u0447\u0430\u0435\u0442, \u0447\u0442\u043e \u0432 \u043f\u0440\u0438\u043e\u0440\u0438\u0442\u0435\u0442\u0435 \u0441\u0442\u0430\u043d\u0435\u0442 \u043e\u043f\u0435\u0440\u0430\u0442\u043e\u0440\u044b, \u0443 \u0438\u0445 \u043d\u0435\u0442 \u043e\u0433\u0440\u0430\u043d\u0438\u0447\u0435\u043d\u0438\u0439 \u0434\u043b\u044f \u0438\u0433\u0440\u043e\u043a\u043e\u0432 \u0432 \u0442\u0432\u043e\u0435\u0439 \u044e\u0440\u0438\u0441\u0434\u0438\u043a\u0446\u0438\u0438. \u041f\u043e\u0441\u0435\u0442\u0438\u0442\u0435\u043b\u0438, \u043a\u043e\u0442\u043e\u0440\u044b\u0445 \u0438\u043d\u0442\u0435\u0440\u0435\u0441\u0443\u044e\u0442 \u0438 \u043d\u0438\u0445 \u043a\u0430\u0437\u0438\u043d\u043e, \u043c\u043e\u0433\u0443\u0442 \u0440\u0430\u0437\u0436\u0438\u0442\u044c\u0441\u044f \u0440\u0430\u0441\u0448\u0438\u0440\u0435\u043d\u043d\u044b\u043c\u0438 \u043d\u0430\u0441\u0442\u0440\u043e\u0439\u043a\u0430\u043c\u0438. \u041d\u0430\u0437\u0432\u0430\u043d\u0438\u0435 \u0440\u0435\u0433\u0443\u043b\u044f\u0442\u043e\u0440\u0430 \u0440\u0430\u0441\u0448\u0438\u0444\u0440\u043e\u0432\u044b\u0432\u0430\u0435\u0442\u0441\u044f \u0436\u0435 Philippine Amusement and Gaming Corporation. \u042d\u0442\u043e \u0430\u0437\u0438\u0430\u0442\u0441\u043a\u0430\u044f \u043e\u0440\u0433\u0430\u043d\u0438\u0437\u0430\u0446\u0438\u044f, \u0441\u0430\u043c\u0443 \u043e\u0442\u0432\u0435\u0447\u0430\u0435\u0442 \u0437\u0430 \u043b\u0438\u0446\u0435\u043d\u0437\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u0435 \u0438 \u0440\u0430\u0437\u0432\u0438\u0442\u0438\u0435 \u0438\u0433\u043e\u0440\u043d\u043e\u0433\u043e \u0431\u0438\u0437\u043d\u0435\u0441\u0430. \u0418\u0437\u043d\u0430\u0447\u0430\u043b\u044c\u043d\u043e \u043d\u0435\u0435 \u043a\u043e\u043d\u0442\u0440\u043e\u043b\u0438\u0440\u043e\u0432\u0430\u043b\u0430 \u0434\u0435\u044f\u0442\u0435\u043b\u044c\u043d\u043e\u0441\u0442\u044c \u043f\u043b\u0430\u0432\u0443\u0447\u0438\u0445 \u0438 \u043d\u0430\u0437\u0435\u043c\u043d\u044b\u0445 \u043a\u0430\u0437\u0438\u043d\u043e. \u0421 \u0440\u0430\u0437\u0432\u0438\u0442\u0438\u0435\u043c \u0440\u0435\u0430\u043b\u044c\u043d\u043e\u0433\u043e \u0433\u0435\u043c\u0431\u043b\u0438\u043d\u0433\u0430 \u0441\u0444\u0435\u0440\u0430 \u0432\u043b\u0438\u044f\u043d\u0438\u044f \u043d\u0430\u0447\u0430\u043b\u0430 \u0432\u043a\u043b\u044e\u0447\u0430\u0442\u044c \u0430 \u0441\u0435\u0431\u044f \u0441\u0430\u0439\u0442\u044b \u043f\u0438\u043f\u043f\u0430\u0440\u0434\u043e\u043c \u0430\u0437\u0430\u0440\u0442\u043d\u044b\u043c\u0438 \u0440\u0430\u0437\u0432\u043b\u0435\u0447\u0435\u043d\u0438\u044f\u043c\u0438.<\/p>\n<h3 id=\"toc-3\">\u0421\u043a\u0430\u0447\u0430\u0442\u044c \u0422\u043e\u0440\u0440\u0435\u043d\u0442 \u041d\u0430 \u0418\u0433\u0440\u043e\u0432\u044b\u0435 \u0410\u0432\u0442\u043e\u043c\u0430\u0442\u044b \u0411\u0435\u0441\u043f\u043b\u0430\u0442\u043d\u043e \u0411\u0435\u0437 \u0420\u0435\u0433\u0438\u0441\u0442\u0440\u0430\u0446\u0438\u0438<\/h3>\n<p>\u0412\u0430\u043c \u0442\u043e\u043b\u044c\u043a\u043e \u043d\u0443\u0436\u043d\u043e \u043f\u0440\u0435\u0434\u043f\u0440\u0438\u043d\u0438\u043c\u0430\u0442\u044c \u043f\u043e\u043b\u0441\u043b\u043e\u0432\u0430 \u043a\u0430\u043a\u0438\u0445 \u043e\u0441\u043e\u0431\u044b\u0445 \u0434\u0435\u0439\u0441\u0442\u0432\u0438\u044f \u0447\u0442\u043e \u0431\u044b \u0438\u0433\u0440\u0430\u0442\u044c \u0441 \u043c\u043e\u0431\u0438\u043b\u044c\u043d\u043e\u0433\u043e \u0443\u0441\u0442\u0440\u043e\u0439\u0441\u0442\u0432\u0430. \u041d\u0430\u0448 \u043f\u043e\u0440\u0442\u0430\u043b \u044f\u0432\u043b\u044f\u0435\u0442\u0441\u044f \u043a\u0440\u043e\u0441\u0441\u043f\u043b\u0430\u0442\u0444\u043e\u0440\u043c\u0435\u043d\u043d\u044b\u043c, \u0431\u043e\u043b\u044c\u0448\u0438\u043d\u0441\u0442\u0432\u043e \u0438\u0433\u0440 \u043f\u0440\u0435\u0434\u0441\u0442\u0430\u0432\u043b\u0435\u043d\u044b \u0432 \u0444\u043e\u0440\u043c\u0430\u0442\u0435 HTML5 \u2013 \u043a\u043e\u0442\u043e\u0440\u044b\u0435 \u0441\u043b\u043e\u0442\u044b \u0440\u0430\u0431\u043e\u0442\u0430\u044e\u0442 \u0432 \u043b\u044e\u0431\u043e\u043c \u0443\u0441\u0442\u0440\u043e\u0439\u0441\u0442\u0432\u0435 \u0436\u0435 \u0432 \u043b\u044e\u0431\u043e\u043c \u0431\u0440\u0430\u0443\u0437\u0435\u0440\u0435. \u0414\u0430\u043d\u043d\u0430\u044f \u0430\u043a\u0446\u0438\u044f \u0431\u0435\u0441\u043f\u043b\u0430\u0442\u043d\u0430 \u0438 \u0440\u0430\u0441\u043f\u0440\u043e\u0441\u0442\u0440\u0430\u043d\u044f\u0435\u0442\u0441\u044f \u043d\u0430 \u0432\u0441\u0435\u0445 \u0437\u0430\u0440\u0435\u0433\u0438\u0441\u0442\u0440\u0438\u0440\u043e\u0432\u0430\u043d\u043d\u044b\u0445 \u0433\u0435\u043c\u0431\u043b\u0435\u0440\u043e\u0432, \u043a\u0442\u043e \u043d\u0430\u0439\u0434\u0435\u0442 \u0431\u043e\u043d\u0443\u0441-\u043a\u043e\u0434 \u0438 \u0430\u043a\u0442\u0438\u0432\u0438\u0440\u0443\u0435\u0442 \u0438 \u0432 \u0441\u043f\u0435\u0446\u0438\u0430\u043b\u044c\u043d\u043e\u043c \u043f\u043e\u043b\u0435. \u0412\u0430\u0436\u043d\u043e, \u0447\u0442\u043e \u0441\u043a\u0430\u0447\u0430\u0442\u044c \u043f\u0440\u0438\u043b\u043e\u0436\u0435\u043d\u0438\u0435 \u043c\u043e\u0436\u043d\u043e \u0436\u0435 \u043d\u0430 \u043f\u043b\u0430\u043d\u0448\u0435\u0442 \u043d\u0430\u043f\u0440\u0438\u043c\u0435\u0440 \u041f\u041a. \u0427\u0442\u043e\u0431\u044b \u0437\u0430\u0433\u0440\u0443\u0437\u0438\u0442\u044c \u043f\u043b\u0430\u0433\u0438\u043d \u043d\u0430 \u043f\u043b\u0430\u043d\u0448\u0435\u0442 \u0438\u043b\u0438 \u041f\u041a \u043d\u0430\u0447\u0438\u043d\u0430\u044e\u0442 \u0432 \u0440\u0430\u0437\u0434\u0435\u043b \u00ab\u0421\u043a\u0430\u0447\u0438\u0432\u0430\u0435\u043c\u044b\u0435 \u043a\u043b\u0438\u0435\u043d\u0442\u044b\u00bb, \u0437\u0430\u0445\u043e\u0434\u044f \u043d\u0430 \u0441\u0430\u0439\u0442 \u0447\u0435\u0440\u0435\u0437 \u043a\u043e\u043c\u043f\u044c\u044e\u0442\u0435\u0440 \u0438\u043b\u0438 \u0434\u0440\u0443\u0433\u043e\u0435 \u0443\u0441\u0442\u0440\u043e\u0439\u0441\u0442\u0432\u043e. \u0417\u0430\u0433\u0440\u0443\u0437\u043a\u0430 \u043f\u0440\u043e\u0438\u0441\u0445\u043e\u0434\u0438\u0442 \u0441 \u0441\u0430\u0439\u0442\u0430 \u0438\u043b\u0438 \u0437\u0435\u0440\u043a\u0430\u043b\u0430 \u043d\u0430\u043f\u0440\u044f\u043c\u0443\u044e, \u043d\u0435 \u043f\u0440\u043e\u0448\u043b\u043e \u0442\u043e\u0440\u0440\u0435\u043d\u0442.<\/p>\n<ul>\n<li>\u0415\u0441\u043b\u0438 \u043e\u043f\u0435\u0440\u0430\u0442\u043e\u0440 \u043e\u0437\u0430\u0431\u043e\u0442\u0438\u043b\u0441\u044f \u043f\u043e\u043b\u0443\u0447\u0435\u043d\u0438\u0435\u043c \u0434\u043e\u043a\u0443\u043c\u0435\u043d\u0442\u043e\u0432 \u043e\u0442 \u043e\u0434\u043d\u043e\u0439 \u043e\u0440\u0433\u0430\u043d\u0438\u0437\u0430\u0446\u0438\u0438, \u044d\u0442\u043e \u0433\u043e\u0432\u043e\u0440\u044e \u043e&#8221; &#8220;\u0442\u043e\u043c, \u0447\u0442\u043e \u043e\u043d \u043d\u0430\u0446\u0435\u043b\u0435\u043d \u043d\u0430 \u0434\u043e\u043b\u0433\u0443\u044e, \u043d\u0435\u043f\u043b\u043e\u0442\u043d\u0443\u044e \u0438 \u0447\u0435\u0441\u0442\u043d\u0443\u044e \u0440\u0430\u0431\u043e\u0442\u0435.<\/li>\n<li>\u0414\u043e\u043b\u0433\u043e\u0435 \u0432\u0440\u0435\u043c\u044f \u0433\u0435\u0439\u043c\u043f\u043b\u0435\u0439 \u0430\u043f\u043f\u0430\u0440\u0430\u0442\u043e\u0432 \u043e\u0441\u0442\u0430\u0432\u0430\u043b\u043e\u0441\u044c \u043d\u0435\u0438\u0437\u043c\u0435\u043d\u043d\u044b\u043c.<\/li>\n<li>\u041f\u0440\u0438 \u0447\u0442\u043e \u0438\u0433\u0440\u043e\u043a \u0441\u0430\u043c \u043e\u043f\u0440\u0435\u0434\u0435\u043b\u044f\u044e\u0442 \u043a\u043e\u043b\u0438\u0447\u0435\u0441\u0442\u0432\u043e \u0440\u0430\u0443\u043d\u0434\u043e\u0432 \u0441\u043e \u0430\u0432\u0442\u043e\u0437\u0430\u043f\u0443\u0441\u043a\u043e\u043c, \u0447\u0442\u043e\u0431\u044b \u0442\u043e\u043b\u044c\u043a\u043e \u043d\u0430\u0436\u0438\u043c\u0430\u0442\u044c \u0432\u0441\u0435\u0433\u0434\u0430 \u00ab\u041f\u0443\u0441\u043a\u00bb.<\/li>\n<li>\u0412 \u043a\u0430\u0437\u0438\u043d\u043e \u0440\u0430\u0437\u0432\u0435 \u0438\u0433\u0440\u043e\u0432\u044b\u0435 \u0430\u0432\u0442\u043e\u043c\u0430\u0442\u044b \u043d\u0430 \u0431\u0430\u0437\u0435 Flash \u0436\u0435 HTML5.<\/li>\n<li>\u041f\u0440\u0438\u043b\u043e\u0436\u0435\u043d\u0438\u044f \u0438 \u0410\u043d\u0434\u0440\u043e\u0438\u0434 \u0440\u0435\u0434\u043a\u043e \u0434\u043e\u0441\u0442\u0443\u043f\u043d\u0443\u044e \u043a \u0441\u043a\u0430\u0447\u0438\u0432\u0430\u043d\u0438\u044e \u0432 Google Play, \u043d\u043e \u0431\u044b\u0432\u0430\u044e\u0442 \u0438\u0441\u043a\u043b\u044e\u0447\u0435\u043d\u0438\u044f.<\/li>\n<\/ul>\n<p>\u0411\u043e\u043d\u0443\u0441\u044b, \u043a\u043e\u044d\u0444\u0444\u0438\u0446\u0438\u0435\u043d\u0442\u044b, \u043d\u0430\u0441\u0442\u0440\u043e\u0439\u043a\u0438 \u0432 \u043e\u0437\u043d\u0430\u043a\u043e\u043c\u0438\u0442\u0435\u043b\u044c\u043d\u043e\u0439 \u0432\u0435\u0440\u0441\u0438\u0438 \u0442\u0435 \u0436\u0435, \u0447\u0442\u043e \u0436\u0435 \u0432 \u043f\u043e\u043b\u043d\u043e\u0439. \u0427\u0430\u0449\u0435 \u0432\u0441\u0435\u0433\u043e \u043e\u043d\u0438 \u043a\u0430\u0441\u0430\u0435\u0442\u0441\u044f \u0444\u0443\u043d\u043a\u0446\u0438\u0438 \u0440\u043e\u0437\u044b\u0433\u0440\u044b\u0448\u0430 \u0434\u0436\u0435\u043a\u043f\u043e\u0442\u0430. \u0420\u0430\u0437\u0440\u0430\u0431\u043e\u0442\u0447\u0438\u043a\u0438 \u0432\u044b\u043f\u0443\u0441\u043a\u0430\u044e\u0442 \u0434\u0435\u043c\u043e \u0441\u043b\u043e\u0442\u043e\u0432, \u0447\u0442\u043e\u0431\u044b \u0438\u0433\u0440\u043e\u043a \u043e\u0437\u043d\u0430\u043a\u043e\u043c\u0438\u043b\u0441\u044f \u0441 \u043f\u0440\u0430\u0432\u0438\u043b\u0430\u043c\u0438, \u043e\u0446\u0435\u043d\u0438\u043b \u0433\u0440\u0430\u0444\u0438\u043a\u0443 \u0430 \u043e\u0444\u043e\u0440\u043c\u043b\u0435\u043d\u0438\u0435.<\/p>\n<h2 id=\"toc-4\">\u0418\u0433\u0440\u0430\u0442\u044c \u041d\u0430 \u0434\u0435\u043d\u044c\u0433\u0438 \u0418\u043b\u0438 \u0418\u0433\u0440\u0430\u0442\u044c \u0436\u0435\u043b\u0430\u044e\u0449\u0438\u043c \u2013 \u0421\u0440\u0430\u0432\u043d\u0435\u043d\u0438\u0435<\/h2>\n<p>\u0420\u0430\u0437\u0440\u0435\u0448\u0435\u043d\u0438\u0435 \u0443\u0441\u043b\u043e\u0432\u0438\u0435\u043c \u0447\u0435\u0441\u0442\u043d\u044b\u0435 \u0432\u044b\u043f\u043b\u0430\u0442\u044b \u0430 \u043e\u0440\u0438\u0433\u0438\u043d\u0430\u043b\u044c\u043d\u043e\u0441\u0442\u044c \u0441\u043e\u0444\u0442\u0430. \u0414\u0430\u043d\u043d\u044b\u0435 \u043e \u043d\u0435\u043c \u0441\u043e\u0441\u0442\u0430\u0432\u043b\u044f\u044e\u0442 \u0432 \u0444\u0443\u0442\u0435\u0440\u0435 \u0441\u0430\u0439\u0442\u0430 \u043a\u0430\u0437\u0438\u043d\u043e \u0438 \u0441\u043e\u043f\u0440\u043e\u0432\u043e\u0436\u0434\u0430\u044e\u0442\u0441\u044f \u043a\u043b\u0438\u043a\u0430\u0431\u0435\u043b\u044c\u043d\u044b\u043c \u043b\u043e\u0433\u043e, \u043f\u043e \u043a\u043e\u0442\u043e\u0440\u043e\u043c\u0443 \u0438\u0433\u0440\u043e\u043a\u0438 \u0431\u044b\u0442\u044c \u043f\u0440\u043e\u0432\u0435\u0440\u0438\u0442\u044c \u043f\u043e\u0434\u043b\u0438\u043d\u043d\u043e\u0441\u0442\u044c \u0440\u0430\u0437\u0440\u0435\u0448\u0435\u043d\u0438\u044f. \u041f\u043e\u0441\u043b\u0435 \u044d\u0442\u043e\u0433\u043e \u0438 \u043f\u0440\u0438\u043b\u043e\u0436\u0435\u043d\u0438\u0438 \u0431\u0443\u0434\u0443\u0442 \u0434\u043e\u0441\u0442\u0443\u043f\u043d\u044b 2 \u043f\u043e\u0441\u043b\u0435\u0434\u043d\u0438\u0445 \u0438\u0433\u0440\u043e\u0432\u044b\u0445 \u0430\u0432\u0442\u043e\u043c\u0430\u0442\u043e\u0432.<\/p>\n<ul>\n<li>\u0416\u0430\u043d\u0440 \u043f\u043e\u043f\u0443\u043b\u044f\u0440\u0435\u043d \u0431\u043b\u0430\u0433\u043e\u0434\u0430\u0440\u044f \u043b\u0443\u0447\u0448\u0435 \u0437\u043d\u0430\u043a\u043e\u043c\u043e\u043c\u0443 \u0441\u0435\u0442\u0442\u0438\u043d\u0433\u0443.<\/li>\n<li>\u0418\u0433\u0440\u0430\u0442\u044c \u0432 \u0441\u043b\u043e\u0442\u044b \u0447\u0435\u0440\u0435\u0437 \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0443 \u043c\u043e\u0436\u043d\u043e \u0431\u0435\u0441\u043f\u043b\u0430\u0442\u043d\u043e (\u0432 \u0434\u0435\u043c\u043e-\u0440\u0435\u0436\u0438\u043c\u0435) \u0438 \u043d\u0430 \u043f\u043e\u0434\u043b\u0438\u043d\u043d\u044b\u0435 \u0434\u0435\u043d\u044c\u0433\u0438.<\/li>\n<li>\u0418\u0437\u043d\u0430\u0447\u0430\u043b\u044c\u043d\u043e \u043e\u043d\u0430 \u043a\u043e\u043d\u0442\u0440\u043e\u043b\u0438\u0440\u043e\u0432\u0430\u043b\u0430 \u0434\u0435\u044f\u0442\u0435\u043b\u044c\u043d\u043e\u0441\u0442\u044c \u043f\u043b\u0430\u0432\u0443\u0447\u0438\u0445 \u0438 \u043d\u0430\u0437\u0435\u043c\u043d\u044b\u0445 \u043a\u0430\u0437\u0438\u043d\u043e.<\/li>\n<li>\u042d\u0442\u043e \u043f\u043e\u0437\u0432\u043e\u043b\u0438\u0442 \u0432 \u0434\u0430\u043b\u044c\u043d\u0435\u0439\u0448\u0435\u043c \u043f\u043e\u043b\u0443\u0447\u0438\u0442\u044c \u043c\u0438\u043d\u0438\u043c\u0443\u043c\u0435 \u0432\u044b\u043f\u043b\u0430\u0442\u044b \u043e\u0442 \u0432\u044b\u0431\u0440\u0430\u043d\u043d\u043e\u0435 \u0441\u043b\u043e\u0442\u0430 \u0432 \u0438\u0433\u0440\u0435 \u043d\u0430 \u0440\u0435\u0430\u043b\u044c\u043d\u044b\u0435 \u043a\u0432\u0430\u0440\u043f\u043b\u0430\u0442\u0443.<\/li>\n<li>\u0410 \u0442\u043e\u0433\u043e \u043d\u0435<\/li>\n<\/ul>\n<p>\u041b\u0435\u0433\u0430\u043b\u044c\u043d\u044b\u0435 \u0438 \u043f\u0440\u043e\u0432\u0435\u0440\u0435\u043d\u043d\u044b\u0435 \u043f\u0440\u043e\u0432\u0430\u0439\u0434\u0435\u0440\u044b \u0438\u0433\u0440\u043e\u0432\u044b\u0445 \u0430\u0432\u0442\u043e\u043c\u0430\u0442\u043e\u0432 \u043f\u0440\u0435\u0434\u043b\u0430\u0433\u0430\u044e\u0442 \u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u0442\u0435\u043b\u044f\u043c \u0441\u043a\u0430\u0447\u0430\u0442\u044c \u0442\u0432\u043e\u0438 \u0441\u043b\u043e\u0442\u044b \u043f\u0440\u044f\u043c\u043e \u0441\u043f\u0443\u0441\u0442\u044f \u0438\u0445 \u043e\u0444\u0438\u0446\u0438\u0430\u043b\u044c\u043d\u044b\u0439 \u0441\u0430\u0439\u0442. \u0427\u0442\u043e\u0431\u044b \u0441\u043a\u0430\u0447\u0430\u0442\u044c \u043f\u0440\u0438\u043b\u043e\u0436\u0435\u043d\u0438\u0435 \u0438\u0433\u0440\u044b \u043d\u0430 \u0410\u043d\u0434\u0440\u043e\u0438\u0434 \u043d\u0430 \u0440\u0443\u0441\u0441\u043a\u043e\u043c \u044f\u0437\u044b\u043a\u0435, \u0432\u0430\u043c \u043d\u0443\u0436\u043d\u043e \u0437\u0430\u0442\u0435\u043c \u043d\u0430\u0439\u0442\u0438 \u0443\u0441\u0442\u0430\u043d\u043e\u0432\u043e\u0447\u043d\u044b\u0439 apk \u0444\u0430\u0439\u043b \u0432 \u043f\u0440\u0438\u043b\u0438\u0447\u043d\u043e\u043c \u0444\u043e\u0440\u043c\u0430\u0442\u0435. \u0417\u0430\u0442\u0435\u043c \u043d\u0430\u0436\u0430\u0442\u044c \u043a\u043d\u043e\u043f\u043a\u0443 \u00ab\u0421\u043a\u0430\u0447\u0430\u0442\u044c\u00bb \u0430 \u043d\u0435\u043c\u043d\u043e\u0433\u043e \u043f\u043e\u0434\u043e\u0436\u0434\u0430\u0442\u044c, \u043f\u043e\u043a\u0443\u0434\u0430 \u0438\u0433\u0440\u043e\u0432\u043e\u0439 \u0430\u043f\u043f\u0430\u0440\u0430\u0442 \u0437\u0430\u0433\u0440\u0443\u0437\u0438\u0442\u044c\u0441\u044f \u043d\u0430 Android.<\/p>\n<h3 id=\"toc-5\">\ud83c\udf88 \u0427\u0442\u043e \u0422\u0430\u043a\u043e\u0435 \u0414\u0435\u043c\u043e \u0412\u0435\u0440\u0441\u0438\u0438 \u0421\u043b\u043e\u0442\u043e\u0432 \u043d\u0435\u0442 \u0420\u0435\u0433\u0438\u0441\u0442\u0440\u0430\u0446\u0438\u0438 \u0418 \u0434\u0434\u044f \u0427\u0435\u0433\u043e \u041e\u043d\u0438 \u043f\u0440\u0435\u0434\u043d\u0430\u0437\u043d\u0430\u0447\u0435\u043d\u043d\u044b\u0435?<\/h3>\n<p>\u041f\u043e\u0441\u043b\u0435 \u0432\u044b\u043f\u043e\u043b\u043d\u0435\u043d\u0438\u044f \u0443\u0441\u0442\u0430\u043d\u043e\u0432\u043a\u0438 \u043d\u0430 \u0440\u0430\u0431\u043e\u0447\u0435\u043c \u0441\u0442\u043e\u043b\u0435 \u043f\u043e\u044f\u0432\u0438\u0442\u0441\u044f \u044f\u0440\u043b\u044b\u043a \u0434\u043b\u044f \u0437\u0430\u043f\u0443\u0441\u043a\u0430 \u043a\u043b\u0438\u0435\u043d\u0442\u0430. \u0411\u043b\u0430\u0433\u043e\u0434\u0430\u0440\u044f \u043c\u043d\u043e\u0433\u043e\u0447\u0438\u0441\u043b\u0435\u043d\u043d\u044b\u043c \u0432\u044b\u0448\u0435\u043f\u0435\u0440\u0435\u0447\u0438\u0441\u043b\u0435\u043d\u043d\u043e\u043c\u0443 \u0442\u0432\u043e\u0439 \u0431\u0430\u043b\u0430\u043d\u0441 \u043a\u0440\u0435\u0434\u0438\u0442\u044b Gaminator \u0431\u0443\u0434\u0435\u0442 \u0440\u0435\u0433\u0443\u043b\u044f\u0440\u043d\u043e \u043f\u043e\u043f\u043e\u043b\u043d\u044f\u0442\u044c\u0441\u044f. \u0412 \u0447\u0442\u043e \u0440\u0430\u0437\u0434\u0435\u043b\u0435 \u0442\u044b \u0443\u0437\u043d\u0430\u0435\u0442, \u043a\u0430\u043a\u0438\u0435 \u0431\u043e\u043d\u0443\u0441\u044b \u044f \u0434\u043e\u0441\u0442\u0443\u043f\u043d\u044b \u0438 \u0430 \u0432 \u0446\u0435\u043b\u043e\u043c \u0443\u0441\u0442\u0440\u043e\u0435\u043d\u0430 \u0441\u0438\u0441\u0442\u0435\u043c\u0430 \u0431\u043e\u043d\u0443\u0441\u043e\u0432.<\/p>\n<p>\u0421\u0442\u0440\u0430\u0442\u0435\u0433\u0438\u044f \u0444\u043e\u0440\u043c\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u044f \u0431\u0430\u043d\u043a\u0440\u043e\u043b\u043b\u0430 &#8211; \u044d\u0442\u043e \u0438\u0433\u0440\u0430 \u043d\u0430 \u043d\u0435\u0431\u043e\u043b\u044c\u0448\u0438\u0435 \u0441\u0442\u0430\u0432\u043a\u0438 \u0432 \u043d\u0430\u0447\u0430\u043b\u0435. \u042d\u0442\u043e \u0442\u0430\u043a\u0436\u0435 \u0431\u0435\u0437\u043e\u043f\u0430\u0441\u043d\u043e \u0438 \u0442\u043e\u043c \u0441\u043c\u044b\u0441\u043b\u0435, \u0447\u0442\u043e \u0435\u0441\u043b\u0438 \u0432\u044b \u0432\u044b\u0438\u0433\u0440\u0430\u044e\u0442, \u044d\u0442\u043e \u043d\u0435 \u0441\u0438\u043b\u044c\u043d\u0435\u0435 \u043f\u043e\u0432\u0440\u0435\u0434\u0438\u0442 \u0432\u0430\u0448\u0438\u043c \u0444\u0438\u043d\u0430\u043d\u0441\u043e\u0432\u044b\u043c \u0446\u0435\u043b\u044f\u043c. \u041f\u043e\u0441\u043b\u0435 \u044d\u0442\u043e\u0433\u043e, \u043a\u0430\u043a \u0431\u0443\u0434\u0435\u0442 \u043f\u0440\u0435\u0434\u043d\u0430\u0437\u043d\u0430\u0447\u0435\u043d\u0430 \u043f\u043e\u0434\u0443\u0448\u043a\u0430 \u0431\u0435\u0437\u043e\u043f\u0430\u0441\u043d\u043e\u0441\u0442\u0438, \u0438\u0433\u0440\u0430\u0439\u0442\u0435 \u043f\u043e-\u043a\u0440\u0443\u043f\u043d\u043e\u043c\u0443. \u041f\u0440\u043e\u0438\u0437\u0432\u043e\u0434\u0438\u0442\u0435\u043b\u044c \u0442\u043e\u043b\u044c\u043a\u043e \u043c\u043e\u0436\u0435\u0442 \u043f\u043e\u0445\u0432\u0430\u0441\u0442\u0430\u0442\u044c\u0441\u044f \u0434\u0432\u043e\u0439\u043d\u044b\u043c \u043a\u043e\u043b\u0438\u0447\u0435\u0441\u0442\u0432\u043e\u043c \u0440\u0430\u0437\u0440\u0430\u0431\u043e\u0442\u0430\u043d\u043d\u044b\u0445 \u0430\u0432\u0442\u043e\u043c\u0430\u0442\u043e\u0432. \u0412 \u043f\u043e\u0441\u043b\u0435\u0434\u043d\u0438\u0435 \u0435\u0449\u0435 \u043b\u0435\u0442 \u043a\u043e\u043c\u043f\u0430\u043d\u0438\u0438 \u0441\u0443\u043c\u0435\u043b \u0441\u0434\u0435\u043b\u0430\u0442\u044c \u043a\u0430\u0447\u0435\u0441\u0442\u0432\u0435\u043d\u043d\u044b\u0439 \u043f\u043e\u0441\u0442\u0435\u043f\u0435\u043d\u043d\u044b\u0439 \u0432\u043f\u0435\u0440\u0435\u0434. \u041e\u0441\u043e\u0431\u043e\u0433\u043e \u0432\u043d\u0438\u043c\u0430\u043d\u0438\u044f \u0437\u0430\u0441\u043b\u0443\u0436\u0438\u0432\u0430\u044e\u0442 \u0438\u0433\u0440\u044b Winterberries, Super Heroes \u0436\u0435 Nirvana.<\/p>\n<h3 id=\"toc-6\">\u0418\u0433\u0440\u043e\u0432\u043e\u0439 \u0410\u0432\u0442\u043e\u043c\u0430\u0442 Aztec Gold<\/h3>\n<p>\u041a\u0430\u0437\u0438\u043d\u043e Sol \u043f\u0440\u0438\u043d\u0430\u0434\u043b\u0435\u0436\u0438\u0442 \u0442\u043e\u043c\u0443 \u0436\u0435 \u043e\u043f\u0435\u0440\u0430\u0442\u043e\u0440\u0443,  \u0447\u0442\u043e \u0438 Fresh. \u0412 \u043a\u0430\u0442\u0430\u043b\u043e\u0433\u0435 \u0434\u043e\u0441\u0442\u0443\u043f\u043d\u044b \u0430\u043f\u043f\u0430\u0440\u0430\u0442\u044b \u043e\u0442 \u043f\u043e\u043f\u0443\u043b\u044f\u0440\u043d\u044b\u0445 \u0440\u0430\u0437\u0440\u0430\u0431\u043e\u0442\u0447\u0438\u043a\u043e\u0432, \u043b\u0443\u0447\u0448\u0438\u0445 \u043a\u0430\u043a Pragmatic Play, NetEnt, Endorphina \u0436\u0435 \u0434\u0440\u0443\u0433\u0438\u0435. \u0411\u043e\u043d\u0443\u0441\u044b \u0430 \u0444\u0440\u0438\u0441\u043f\u0438\u043d\u044b \u0434\u0430\u044e\u0442\u0441\u044f \u043d\u0435 \u0442\u043e\u043b\u044c\u043a\u043e \u0437\u0430 \u0430\u0440\u0445\u0438\u043c\u0430\u043b\u043e, \u043d\u043e \u0438 \u0437\u0430 \u043f\u043e\u0441\u043b\u0435\u0434\u0443\u044e\u0449\u0438\u0435 \u043f\u043e\u043f\u043e\u043b\u043d\u0435\u043d\u0438\u044f.<\/p>\n<ul>\n<li>\u0412 \u0444\u0430\u043a\u0442\u0435 \u0436\u0435 \u0433\u043e\u0434\u0443 \u043a\u043e\u043c\u043f\u0430\u043d\u0438\u044f \u0424\u0435\u044f \u043f\u043e\u0441\u0442\u0430\u0432\u0438\u043b\u0430 \u0430\u0432\u0442\u043e\u043c\u0430\u0442\u044b \u043d\u0430 \u043c\u0430\u0441\u0441\u043e\u0432\u043e\u0435 \u043f\u0440\u043e\u0438\u0437\u0432\u043e\u0434\u0441\u0442\u0432\u043e.<\/li>\n<li>\u0412 \u0431\u043e\u043b\u044c\u0448\u0438\u043d\u0441\u0442\u0432\u0435 \u0441\u043b\u043e\u0442\u043e\u0432 \u043f\u043e\u043a\u0430\u0437\u0430\u0442\u0435\u043b\u044c \u0432\u044b\u043f\u043b\u0430\u0442 \u0432\u0430\u0440\u044c\u0438\u0440\u043e\u0432\u0430\u0442\u044c\u0441\u044f \u0432 \u043f\u0440\u0435\u0434\u0435\u043b\u0430\u0445 95-98%.<\/li>\n<li>\u0427\u0442\u043e\u0431\u044b \u0437\u0430\u0438\u043c\u0435\u0442\u044c \u043e\u0434\u0438\u043d \u0438\u0437 \u0441\u0430\u043c\u044b\u0445&#8221; &#8220;\u043f\u0440\u0438\u0437\u043e\u0432, \u043d\u0435\u043e\u0431\u0445\u043e\u0434\u0438\u043c\u043e \u0443\u0434\u0430\u0447\u043d\u043e \u0432\u044b\u043f\u0443\u0441\u0442\u0438\u0442\u044c \u043a\u043e\u043b\u0435\u0441\u043e \u0444\u043e\u0440\u0442\u0443\u043d\u044b.<\/li>\n<li>\u0412 \u043d\u0435\u043c \u043d\u0438\u043a\u043e\u0433\u0434\u0430 \u043c\u043e\u0436\u043d\u043e \u0443\u0437\u043d\u0430\u0442\u044c, \u0441\u043a\u043e\u043b\u044c\u043a\u043e \u0434\u0435\u043d\u0435\u0433 \u043e\u0441\u0442\u0430\u043b\u043e\u0441\u044c \u043d\u0430 \u0441\u0447\u0435\u0442\u0435, \u0432\u044b\u0431\u0440\u0430\u0442\u044c \u0432\u0430\u043b\u044e\u0442\u0443, \u0443\u0441\u0442\u0430\u043d\u043e\u0432\u0438\u0442\u044c \u043b\u0438\u043c\u0438\u0442\u044b \u0436\u0435 \u0434\u0440\u0443\u0433\u0438\u0435 \u043d\u0430\u0441\u0442\u0440\u043e\u0439\u043a\u0438.<\/li>\n<\/ul>\n<p>\u041f\u0430\u0440\u0438\u043c\u0430\u0442\u0447 \u043f\u0440\u0435\u0434\u043b\u0430\u0433\u0430\u0435\u0442 \u0441\u043b\u043e\u0442\u044b \u043e\u0442\u043e \u0430\u0432\u0442\u043e\u0440\u0438\u0442\u0435\u0442\u043d\u044b\u0445 \u043f\u0440\u043e\u0432\u0430\u0439\u0434\u0435\u0440\u043e\u0432. \u0422\u0430\u043a \u043a\u043e\u043c\u043f\u0430\u043d\u0438\u0438 Amatic, Betsoft, Booongo, EGT, Igrosoft, Microgaming, NetEnt, Novomatic, Yggdrasil \u0438 \u043a\u043e\u0442\u043e\u0440\u044b\u0445. \u0421\u0440\u0435\u0434\u0438 \u0434\u043e\u0441\u0442\u043e\u0438\u043d\u0441\u0442\u0432 \u043f\u043b\u043e\u0449\u0430\u0434\u043a\u0438 \u043c\u043e\u0436\u043d\u043e \u0432\u044b\u0434\u0435\u043b\u0438\u0442\u044c \u043e\u0431\u043e\u0440\u043e\u043d\u0438\u0442\u0435\u043b\u044c\u043d\u0443\u044e \u0431\u043e\u043d\u0443\u0441\u043d\u0443\u044e \u0441\u0438\u0441\u0442\u0435\u043c\u0443, \u0430 \u0442\u0430\u043a\u0436\u0435 \u0432\u044b\u0441\u043e\u043a\u043e\u0435 \u043a\u0430\u0447\u0435\u0441\u0442\u0432\u043e \u0441\u0430\u0439\u0442\u0430 \u0438 \u043c\u043e\u0431\u0438\u043b\u044c\u043d\u044b\u0445 \u043f\u0440\u0438\u043b\u043e\u0436\u0435\u043d\u0438\u0439. \u0412 \u043f\u0435\u0440\u0438\u043e\u0434 \u043e\u0442\u0441\u0443\u0442\u0441\u0442\u0432\u0438\u044f \u0438\u043d\u0442\u0435\u0440\u043d\u0435\u0442 \u0443 \u0412\u0430\u0441 \u0431\u0443\u0434\u0435\u0442 \u0432\u043e\u0437\u043c\u043e\u0436\u043d\u043e\u0441\u0442\u044c \u043f\u0440\u0438\u044f\u0442\u043d\u043e \u043f\u0440\u043e\u0432\u0435\u0441\u0442\u0438 \u0441\u0432\u043e\u0439 \u0434\u043e\u0441\u0443\u0433, \u0432\u0435\u0434\u044c \u043f\u043e\u0438\u0433\u0440\u0430\u0442\u044c \u0432 \u043f\u043e\u043f\u0443\u043b\u044f\u0440\u043d\u044b\u0439 \u0438\u0433\u0440\u043e\u0432\u043e\u0439 \u0430\u0432\u0442\u043e\u043c\u0430\u0442 \u043c\u043e\u0436\u043d\u043e \u043d\u0438\u043a\u043e\u0433\u0434\u0430. \u041d\u0435 \u0442\u0440\u0435\u0431\u0443\u0435\u0442\u0441\u044f \u043d\u0435\u0431\u043e\u043b\u044c\u0448\u0438\u0445 \u0443\u0441\u0438\u043b\u0438\u0439 \u0438 \u0437\u043d\u0430\u043d\u0438\u0439 \u0434\u043b\u044f \u0441\u043a\u0430\u0447\u0438\u0432\u0430\u043d\u0438\u044f \u0438 \u0437\u0430\u043f\u0443\u0441\u043a\u0430 \u043d\u0443\u0436\u043d\u043e\u0439 \u0438\u0433\u0440\u044b. \u041c\u043d\u043e\u0433\u0438\u0435 \u0445\u043e\u0442\u044f\u0442 \u043d\u0430\u0447\u0430\u0442\u044c \u0438\u0433\u0440\u0430\u0442\u044c \u0432 \u043a\u0430\u0437\u0438\u043d\u043e, \u043d\u043e \u0431\u043e\u044f\u0442\u0441\u044f \u043b\u0438\u0448\u043d\u0438\u0439 \u0434\u0435\u043d\u044c\u0433\u0430\u043c\u0438.<\/p>\n<h2 id=\"toc-7\">\u041f\u043e\u043b\u043d\u0430\u044f \u0412\u0435\u0440\u0441\u0438\u044f \u0414\u043b\u044f \u0418\u0433\u0440\u044b \u043d\u0430 \u0420\u0435\u0430\u043b\u044c\u043d\u044b\u0435 \u0414\u0435\u043d\u044c\u0433\u0438<\/h2>\n<p>\u0412 \u0438\u0433\u0440\u043e\u0432\u044b\u0445 \u043a\u043b\u0443\u0431\u0430\u0445 \u0438\u0433\u0440\u0430\u0442\u044c \u0431\u0435\u0441\u043f\u043b\u0430\u0442\u043d\u043e \u0440\u0430\u0437\u0440\u0435\u0448\u0430\u043b\u0438 \u043f\u0440\u0430\u043a\u0442\u0438\u0447\u0435\u0441\u043a\u0438 \u0432\u043e \u0432\u0441\u0435 \u0438\u0433\u0440\u044b. \u0418\u0441\u043a\u043b\u044e\u0447\u0435\u043d\u0438\u044f \u0441\u043e\u0441\u0442\u0430\u0432\u043b\u044f\u043b\u0438 \u0441\u043b\u043e\u0442\u044b \u0441 \u0434\u0436\u0435\u043a\u043f\u043e\u0442\u043e\u043c \u0438 \u0438\u0433\u0440\u044b \u0432 \u0440\u0435\u0436\u0438\u043c\u0435 \u0441 \u043c\u0435\u0440\u0442\u0432\u043e\u0433\u043e \u0434\u0438\u043b\u0435\u0440\u043e\u043c. \u041e\u043d\u0438 \u043f\u043e\u0434\u0434\u0435\u0440\u0436\u0438\u0432\u0430\u044e\u0442 \u0442\u043e\u043b\u044c\u043a\u043e \u0438\u0433\u0440\u0443 \u0432 \u0434\u0435\u043d\u044c\u0433\u0438, \u0442\u0430\u043a \u043a\u0430\u043a \u0434\u0430\u044e\u0442 \u0432\u043e\u0437\u043c\u043e\u0436\u043d\u043e\u0441\u0442\u044c \u0437\u0430\u0438\u043c\u0435\u0442\u044c \u043a\u0440\u0443\u043f\u043d\u044b\u0439 \u0432\u044b\u0438\u0433\u0440\u044b\u0448. \u0410 \u043a\u0430\u0442\u0430\u043b\u043e\u0433\u0435 \u043c\u043d\u043e\u0433\u0438\u0445 \u043a\u0430\u0437\u0438\u043d\u043e \u043f\u0440\u0435\u0434\u0441\u0442\u0430\u0432\u043b\u0435\u043d \u0431\u043e\u043b\u044c\u0448\u043e\u0439 \u0432\u044b\u0431\u043e\u0440\u0430 \u0430\u0432\u0442\u043e\u043c\u0430\u0442\u043e\u0432, \u043f\u043e\u044d\u0442\u043e\u043c\u0443 \u0432\u044b\u0431\u0435\u0440\u0435\u0442 \u0441\u043b\u043e\u0442 \u0434\u043b\u044f \u0431\u0435\u0441\u043f\u043b\u0430\u0442\u043d\u043e\u0439 \u0438\u0433\u0440\u044b \u0431\u0443\u0434\u0435\u0442 \u043b\u0435\u0433\u043a\u043e. \u041a\u0430\u0436\u0434\u044b\u0439 \u0432\u0438\u0434\u0435\u043e\u0441\u043b\u043e\u0442 \u0441\u043d\u0430\u0431\u0436\u0430\u0435\u0442\u0441\u044f \u0434\u0435\u043c\u043e\u0432\u0435\u0440\u0441\u0438\u0435\u0439, \u043f\u043e\u044d\u0442\u043e\u043c\u0443 \u0433\u0435\u043c\u0431\u043b\u0435\u0440 \u043c\u043e\u0436\u0435\u0442 \u0438\u0433\u0440\u0430\u0442\u044c \u0432 \u043d\u043e\u0432\u044b\u0435 \u0438\u0433\u0440\u043e\u0432\u044b\u0435 \u0430\u0432\u0442\u043e\u043c\u0430\u0442\u044b \u0431\u0435\u0441\u043f\u043b\u0430\u0442\u043d\u043e \u0438 \u0431\u0435\u0437 \u0440\u0435\u0433\u0438\u0441\u0442\u0440\u0430\u0446\u0438\u0438. \u041f\u0440\u0435\u0434\u0441\u0442\u0430\u0432\u043b\u0435\u043d\u043d\u044b\u0435 \u0438 \u044d\u0442\u043e\u0439 \u0441\u0442\u0440\u0430\u043d\u0438\u0446\u0435 \u0438\u0433\u0440\u044b \u0438\u043c\u0435\u044e\u0442 \u043f\u043e\u0434\u0440\u043e\u0431\u043d\u044b\u0435 \u0440\u0435\u0446\u0435\u043d\u0437\u0438\u0438.<\/p>\n<ul>\n<li>\u0411\u043e\u043b\u044c\u0448\u0438\u043d\u0441\u0442\u0432\u043e \u043d\u043e\u0432\u0438\u043d\u043e\u043a \u0441 \u043f\u0435\u0440\u0432\u044b\u0445 \u043d\u0435\u0441\u043a\u043e\u043b\u044c\u043a\u0441 \u0432\u044b\u0445\u043e\u0434\u0430 \u0434\u043e\u0441\u0442\u0443\u043f\u043d\u044b \u0438 \u0431\u0435\u0441\u043f\u043b\u0430\u0442\u043d\u043e\u043c \u0440\u0435\u0436\u0438\u043c\u0435.<\/li>\n<li>\u041f\u043e\u043b\u043d\u0430\u044f \u0438\u043b\u0438 \u0447\u0430\u0441\u0442\u0438\u0447\u043d\u043e\u0435 \u043a\u043e\u043f\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u0435 \u043c\u0430\u0442\u0435\u0440\u0438\u0430\u043b\u043e\u0432 \u0432\u043e\u0437\u043c\u043e\u0436\u043d\u043e \u0442\u043e\u043b\u044c\u043a\u043e \u0441 \u043f\u0438\u0441\u044c\u043c\u0435\u043d\u043d\u043e\u0433\u043e \u0440\u0430\u0437\u0440\u0435\u0448\u0435\u043d\u0438\u044f \u0440\u0435\u0434\u0430\u043a\u0446\u0438\u0438.<\/li>\n<li>\u0411\u043b\u0430\u0433\u043e\u0434\u0430\u0440\u044f \u043d\u043e\u0432\u044b\u043c \u0442\u0435\u0445\u043d\u043e\u043b\u043e\u0433\u0438\u044f\u043c, \u043c\u043e\u0436\u043d\u043e \u0438\u0433\u0440\u0430\u0442\u044c \u0430 \u043f\u043e\u043f\u0443\u043b\u044f\u0440\u043d\u044b\u0435 \u0441\u043b\u043e\u0442\u044b \u0441\u043e \u0441\u043c\u0430\u0440\u0442\u0444\u043e\u043d\u0430 \u0438\u043b\u0438 \u043f\u043b\u0430\u043d\u0448\u0435\u0442\u0430.<\/li>\n<li>\u041a\u0430\u043a \u0434\u043e\u043f\u043e\u043b\u043d\u0438\u0442\u0435\u043b\u044c\u043d\u044b\u0439 \u0440\u0430\u0443\u043d\u0434, \u043e\u043d \u0430\u043a\u0442\u0438\u0432\u0438\u0440\u0443\u0435\u0442\u0441\u044f \u043f\u043e\u0441\u043b\u0435 \u0441\u043e\u0441\u0442\u0430\u0432\u043b\u0435\u043d \u0432\u044b\u0438\u0433\u0440\u044b\u0448\u043d\u043e\u0439 \u043a\u043e\u043c\u0431\u0438\u043d\u0430\u0446\u0438\u0438.<\/li>\n<\/ul>\n<p>\u041f\u0440\u0438 \u044d\u0442\u043e\u043c \u043f\u0440\u043e\u0446\u0435\u0441\u0441 \u043d\u0435 \u0431\u044b\u043b\u043e \u0447\u0435\u043c-\u043b\u0438\u0431\u043e \u043e\u0442\u043b\u0438\u0447\u0430\u0442\u044c\u0441\u044f \u043e\u0442 \u0438\u0433\u0440\u044b \u0437\u0430 \u043a\u043e\u043c\u043f\u044c\u044e\u0442\u0435\u0440\u043e\u043c \u0438\u043b\u0438 \u043d\u043e\u0443\u0442\u0431\u0443\u043a\u043e\u043c. \u0418\u0437\u0432\u0435\u0441\u0442\u043d\u0430\u044f \u0430\u0432\u0441\u0442\u0440\u0438\u0439\u0441\u043a\u0430\u044f \u043a\u043e\u043c\u043f\u0430\u043d\u0438\u044f \u0437\u0430\u0441\u043b\u0443\u0436\u0435\u043d\u043d\u043e \u0432\u0445\u043e\u0434\u0438\u0442 \u0432 \u043a\u043e\u043b\u0438\u0447\u0435\u0441\u0442\u0432\u043e \u0432\u0435\u0434\u0443\u0449\u0438\u0445 \u043c\u0438\u0440\u043e\u0432\u044b\u0445 \u043f\u0440\u043e\u0438\u0437\u0432\u043e\u0434\u0438\u0442\u0435\u043b\u0435\u0439 \u0438\u0433\u0440\u043e\u0432\u043e\u0433\u043e \u0441\u043e\u0444\u0442\u0430. \u041f\u0440\u043e\u0432\u0430\u0439\u0434\u0435\u0440 \u043e\u0447\u0435\u043d\u044c \u0441\u0435\u0440\u044c\u0435\u0437\u043d\u043e \u0434\u0432\u0438\u0436\u0435\u0442\u0441\u044f \u043a \u0432\u044b\u0431\u043e\u0440\u0443 \u043f\u0430\u0440\u0442\u043d\u0435\u0440\u043e\u0432 \u0441\u0440\u0435\u0434\u0438 \u043e\u043d\u043b\u0430\u0439\u043d-\u043a\u0430\u0437\u0438\u043d\u043e \u0436\u0435 \u0441\u043e\u0442\u0440\u0443\u0434\u043d\u0438\u0447\u0430\u0435\u0442 \u0441 \u0430\u0432\u0442\u043e\u0440\u0438\u0442\u0435\u0442\u043d\u044b\u043c\u0438 \u0437\u0430\u0432\u0435\u0434\u0435\u043d\u0438\u044f\u043c\u0438. \u0421\u0440\u0435\u0434\u0438 \u0437\u043d\u0430\u043c\u0435\u043d\u0438\u0442\u044b\u0445 \u0438\u0433\u0440 \u043c\u043e\u0436\u043d\u043e \u043e\u0442\u0434\u0435\u043b\u044c\u043d\u0443\u044e Book of Ra, Sizzling Hot \u0438 Lucky Lady&#8217;s Charm. \u0415\u0449\u0435 \u0431\u044b\u0432\u0430\u044e\u0442 \u043c\u043d\u043e\u0433\u043e\u0443\u0440\u043e\u0432\u043d\u0435\u0432\u044b\u0435 \u0431\u043e\u043d\u0443\u0441\u044b, \u0433\u0434\u0435 \u0438\u0433\u0440\u043e\u043a\u0443 \u043d\u0435\u043e\u0431\u0445\u043e\u0434\u0438\u043c\u043e \u0432\u044b\u043f\u043e\u043b\u043d\u0438\u0442\u044c \u0440\u044f\u0434 \u0443\u0441\u043b\u043e\u0432\u0438\u0439. \u0412 \u043d\u0435\u043c\u043d\u043e\u0433\u0438\u0445 \u0441\u043b\u043e\u0442\u0430\u0445 \u0431\u043e\u043d\u0443\u0441\u043d\u0430\u044f \u0444\u0443\u043d\u043a\u0446\u0438\u044f \u0440\u0435\u0430\u043b\u0438\u0437\u0443\u0435\u0442\u0441\u044f \u0441 \u043f\u043e\u043c\u043e\u0449\u044c\u044e \u043a\u043e\u043b\u0435\u0441\u0430 \u0444\u043e\u0440\u0442\u0443\u043d\u044b, \u0441\u043e\u043b\u0435\u043d\u0441\u043a \u0432\u043e\u0437\u043c\u043e\u0436\u0435\u043d \u043a\u0430\u043a \u0434\u043e\u0441\u0442\u0430\u0442\u043e\u0447\u043d\u044b\u0439, \u0442\u0430\u043a \u0438 \u043e\u0447\u0435\u043d\u044c \u043a\u0440\u0443\u043f\u043d\u044b\u0439 \u0432\u044b\u0438\u0433\u0440\u044b\u0448.<\/p>\n<h2 id=\"toc-8\">\u0418\u0433\u0440\u0430 \u0412 \u0414\u0435\u043c\u043e \u0418\u0433\u0440\u043e\u0432\u044b\u0445 \u0410\u0432\u0442\u043e\u043c\u0430\u0442\u043e\u0432 \u0432 \u0422\u0435\u043b\u0435\u0444\u043e\u043d\u0430\u0445 Android, Ios<\/h2>\n<p>\u0412\u044b \u043f\u043e\u043d\u0438\u043c\u0430\u044e\u0442, \u043a\u0430\u043a \u0432\u044b\u0431\u0438\u0440\u0430\u0442\u044c \u0438\u0433\u0440\u043e\u0432\u044b\u0435 \u0430\u0432\u0442\u043e\u043c\u0430\u0442\u044b \u043e\u043d\u043b\u0430\u0439\u043d \u0441\u043e \u0431\u043e\u043d\u0443\u0441\u043d\u044b\u043c\u0438 \u0443\u0440\u043e\u0432\u043d\u044f\u043c\u0438. \u0422\u0435\u043f\u0435\u0440\u044c, \u043a\u043e\u0433\u0434\u0430 \u0432\u044b \u0432\u043e\u0448\u0435\u043b \u0432\u043e \u0432\u043a\u0443\u0441, \u0441\u0430\u043c\u0438 \u0432\u043e\u0432\u0441\u044e \u0434\u0435\u043b\u0430\u0435\u0442\u0435 \u0441\u0442\u0430\u0432\u043a\u0438 \u0438 \u043e\u0436\u0438\u0434\u0430\u0435\u0442\u0435 \u0431\u043e\u043b\u044c\u0448\u0438\u0445 \u043e\u0442\u0434\u0430\u0447. \u041a\u043e\u0433\u0434\u0430 \u0432\u044b \u0432\u044b\u0438\u0433\u0440\u044b\u0432\u0430\u0435\u0442\u0435 \u0440\u0430\u0443\u043d\u0434, \u0443\u0431\u0435\u0440\u0438\u0442\u0435 \u043f\u0440\u0438\u0431\u044b\u043b\u044c, \u0447\u0442\u043e\u0431\u044b \u043f\u0435\u0440\u0435\u0432\u043e\u0434\u0438\u043b \u0435\u0435 \u043d\u0430 \u0442\u0432\u043e\u0439 \u0441\u0447\u0435\u0442 \u0432 \u0431\u0430\u043d\u043a, \u0430 \u043e\u0441\u0442\u0430\u0432\u0448\u0438\u0435\u0441\u044f \u043f\u0440\u0438\u043b\u0438\u0447\u043d\u044b\u0435 \u043f\u0443\u0441\u043a\u0430\u0439\u0442\u0435 \u0432 \u043e\u0431\u043e\u0440\u043e\u0442. \u0422\u0430\u043a \u0432\u044b \u043c\u043e\u0436\u0435\u0442\u0435 \u0441\u043e\u0445\u0440\u0430\u043d\u044f\u0442\u044c \u0432\u0430\u0448 \u0431\u0430\u043d\u043a\u0440\u043e\u043b\u043b \u043d\u0435\u0438\u0437\u043c\u0435\u043d\u043d\u044b\u043c, \u0447\u0442\u043e \u0441\u043c\u043e\u0436\u0435\u043c \u0432\u0430\u043c \u043d\u0430\u0441\u043b\u0430\u0436\u0434\u0430\u0442\u044c\u0441\u044f \u0447\u0438\u0441\u0442\u043e\u043c \u0432\u044b\u0438\u0433\u0440\u044b\u0448\u0435\u043c. \u041d\u0435\u0432\u0430\u0436\u043d\u043e, \u0438\u0433\u0440\u0430\u0435\u0442\u0435 \u043b\u0438 \u0432\u044b \u0430 \u0431\u0430\u0437\u043e\u0432\u044b\u0439 \u0438\u0433\u0440\u043e\u0432\u043e\u0439 \u0430\u0432\u0442\u043e\u043c\u0430\u0442 \u0438\u043b\u0438 \u0432 \u0442\u0435\u043c\u0430\u0442\u0438\u0447\u0435\u0441\u043a\u0438\u0439, \u044d\u0442\u0438 \u0441\u0442\u0440\u0430\u0442\u0435\u0433\u0438\u0438 \u043f\u0440\u0438\u043c\u0435\u043d\u0438\u043c\u044b \u0432 \u043b\u044e\u0431\u043e\u043c \u0442\u0430\u043a\u043e\u043c. \u041f\u0435\u0440\u0435\u0434 \u0442\u0435\u043c, \u0436\u0435 \u0438\u0433\u0440\u0430\u0442\u044c \u0432 \u0431\u0435\u0441\u043f\u043b\u0430\u0442\u043d\u044b\u0435 \u0438\u0433\u0440\u043e\u0432\u044b\u0435 \u0430\u0432\u0442\u043e\u043c\u0430\u0442\u044b, \u0432\u044b \u043d\u0443\u0436\u043d\u043e \u043f\u043e\u0437\u043d\u0430\u043a\u043e\u043c\u0438\u0442\u044c\u0441\u044f \u0441\u043e \u043b\u0438\u043d\u0438\u044f\u043c\u0438 \u0432\u044b\u043f\u043b\u0430\u0442.<\/p>\n<ul>\n<li>\u041a\u043e\u043b\u043b\u0435\u043a\u0446\u0438\u044f \u0432\u043a\u043b\u044e\u0447\u0430\u0435\u0442 \u0432 \u0441\u0435\u0431\u0435 \u043f\u043e\u043f\u0443\u043b\u044f\u0440\u043d\u044b\u0435 \u0432\u0438\u0434\u0435\u043e\u0441\u043b\u043e\u0442\u044b \u0430 \u0430\u0437\u0430\u0440\u0442\u043d\u044b\u0435 \u0440\u0430\u0437\u0432\u043b\u0435\u0447\u0435\u043d\u0438\u044f, \u043a\u043e\u0442\u043e\u0440\u044b\u0435 \u0438\u0437\u0432\u0435\u0441\u0442\u043d\u044b \u0443\u0437\u043a\u043e\u043c\u0443 \u043a\u0440\u0443\u0433\u0443 \u0438\u0433\u0440\u043e\u043a\u043e\u0432.<\/li>\n<li>\u0412\u044b \u043c\u043e\u0436\u0435\u0442\u0435 \u0438\u0433\u0440\u0430\u0442\u044c \u043a\u0430\u043a \u0441 \u0433\u043e\u0442\u043e\u0432 \u0441\u0445\u0435\u043c\u043e\u0439 \u0443\u043f\u0440\u0430\u0432\u043b\u0435\u043d\u0438\u044f, \u0436\u0435 \u0438 \u0441 \u043f\u0435\u0440\u0441\u043e\u043d\u0430\u043b\u0438\u0437\u0438\u0440\u043e\u0432\u0430\u043d\u043d\u043e\u0439, \u0434\u043e\u0431\u0430\u0432\u0438\u0432 \u043d\u043e\u0432\u044b\u0435 \u043a\u043e\u043d\u0442\u0440\u043e\u043b\u043b\u0435\u0440\u044b \u043f\u043e \u0441\u0432\u043e\u0435\u043c\u0443 \u0441\u043e\u0431\u0441\u0442\u0432\u0435\u043d\u043d\u043e\u043c\u0443 \u0432 \u0441\u043f\u0435\u0446\u0438\u0430\u043b\u044c\u043d\u043e\u043c \u0440\u0435\u0434\u0430\u043a\u0442\u043e\u0440\u0435.<\/li>\n<li>\u041f\u043e\u044d\u0442\u043e\u043c\u0443 \u0443\u0434\u043e\u0431\u043d\u044b\u0439 \u0432\u044b\u0431\u043e\u0440 \u0437\u0430\u0432\u0438\u0441\u0438\u0442 \u043e\u0442 \u043f\u0440\u0438\u043e\u0440\u0438\u0442\u0435\u0442\u043e\u0432 \u043a\u043e\u043d\u043a\u0440\u0435\u0442\u043d\u043e\u0433\u043e \u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u0442\u0435\u043b\u044f.<\/li>\n<li>\u0414\u043e\u0432\u043e\u043b\u044c\u0441\u0442\u0432\u043e\u0432\u0430\u0442\u044c\u0441\u044f \u043c\u043e\u0431\u0438\u043b\u044c\u043d\u044b\u043c \u0442\u0435\u043b\u0435\u0444\u043e\u043d\u043e\u043c \u0434\u043e\u0432\u043e\u043b\u044c\u043d\u043e \u0443\u0434\u043e\u0431\u043d\u043e, \u043f\u043e\u0441\u043a\u043e\u043b\u044c\u043a\u0443 \u043d\u0435\u043b\u044c\u0437\u044f \u0438\u0433\u0440\u0430\u0442\u044c \u0432 \u044d\u0442\u043e\u043c \u043c\u0435\u0441\u0442\u0435.<\/li>\n<li>\u0415\u0441\u043b\u0438 \u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u0442\u0435\u043b\u0438 \u043f\u0438\u0448\u0443\u0442 \u043e \u0437\u0430\u0434\u0435\u0440\u0436\u043a\u0430\u0445 \u0441\u0443\u043c\u043c\u044b \u0438 \u043e\u0431\u043c\u0430\u043d\u0435, \u0447\u0435\u043c \u0441\u043a\u0430\u0447\u0430\u0442\u044c \u043f\u0440\u0438\u043b\u043e\u0436\u0435\u043d\u0438\u0435 \u0441 \u0434\u0440\u0443\u0433\u043e\u0433\u043e \u0441\u0430\u0439\u0442\u0430.<\/li>\n<\/ul>\n<p>\u0422\u0430\u043a \u043d\u0435 \u0431\u044b\u043b \u0438\u0433\u0440\u043e\u0432\u043e\u0439 \u0430\u0432\u0442\u043e\u043c\u0430\u0442 \u0432 \u043f\u043e\u043b\u043d\u043e\u043c \u043f\u043e\u043d\u0438\u043c\u0430\u043d\u0438\u0438 \u044d\u0442\u043e\u0433\u043e \u0442\u0435\u0440\u043c\u0438\u043d\u044b. \u041f\u0440\u043e\u0438\u0437\u0432\u043e\u0434\u0438\u0442\u0435\u043b\u044c Bally Entertainment, \u043a\u043e\u0442\u043e\u0440\u044b\u0439 \u0440\u0430\u0431\u043e\u0442\u0430\u0435\u0442 \u0438 \u043f\u043e \u0441\u0435\u0439 \u0434\u0435\u043d\u044c, \u0432\u044b\u043f\u0443\u0441\u0442\u0438\u043b \u043f\u044f\u0442\u0438\u043a\u0430\u0440\u0442\u043e\u0447\u043d\u0443\u044e \u043f\u043e\u043a\u0435\u0440-\u043c\u0430\u0448\u0438\u043d\u0443. \u0414\u0432\u0430\u0434\u0446\u0430\u0442\u044c\u044e \u0433\u043e\u0434\u0430\u043c\u0438 \u043f\u043e\u0437\u0436\u0435 \u0442\u043e\u0442 \u0436\u0435 \u0431\u0440\u0435\u043d\u0434 \u044f\u0432\u0438\u043b \u043c\u0438\u0440\u0443 \u043e\u0434\u0438\u043d \u0432 \u0438\u0441\u0442\u043e\u0440\u0438\u0438 \u044d\u043b\u0435\u043a\u0442\u0440\u043e\u043c\u0435\u0445\u0430\u043d\u0438\u0447\u0435\u0441\u043a\u0438\u0439 \u0441\u043b\u043e\u0442. \u041a\u0430\u0442\u0443\u0448\u043a\u0438 \u0430\u043f\u043f\u0430\u0440\u0430\u0442\u0430 Money Honey \u043f\u0440\u0438\u0432\u043e\u0434\u0438\u043b\u0438\u0441\u044c \u0432 \u0434\u0432\u0438\u0436\u0435\u043d\u0438\u0435 \u043f\u0438\u043f\u043f\u0430\u0440\u0434\u043e\u043c \u043f\u043e\u043c\u043e\u0449\u044c\u044e \u044d\u043b\u0435\u043a\u0442\u0440\u043e\u043f\u0440\u0438\u0432\u043e\u0434\u0430, \u0436\u0435 \u0441\u0430\u043c\u043e \u0443\u0441\u0442\u0440\u043e\u0439\u0441\u0442\u0432\u043e \u043e\u043a\u0430\u0437\u0430\u043b\u043e\u0441\u044c \u0441\u043d\u0430\u0431\u0436\u0435\u043d\u043e \u043c\u043e\u043d\u0435\u0442\u043d\u044b\u043c \u043d\u0430\u043a\u043e\u043f\u0438\u0442\u0435\u043b\u0435\u043c. \u0415\u0441\u043b\u0438 \u0432\u044b \u0432\u0430\u0441 \u0437\u0430\u043f\u0443\u0441\u0442\u0438\u0442\u044c Flash \u0438\u0433\u0440\u044b \u043d\u0430 \u043c\u043e\u0431\u0438\u043b\u044c\u043d\u043e\u043c \u0442\u0430\u043a\u0436\u0435 \u0443 \u0432\u0430\u0441 \u0435\u0441\u0442\u044c \u043f\u0440\u043e\u0431\u043b\u0435\u043c\u044b \u0441 \u0437\u0430\u043f\u0443\u0441\u043a\u043e\u043c Flash \u043d\u0430 PC, \u0442\u043e \u043e\u0437\u043d\u0430\u043a\u043e\u043c\u044c\u0442\u0435\u0441\u044c \u0436\u0435 \u0441\u0442\u0430\u0442\u044c\u0435\u0439 \u2013 \u00ab\u043a\u0430\u043a \u0437\u0430\u043f\u0443\u0441\u0442\u0438\u0442\u044c Flash \u0438&#8221; &#8220;2021 \u0433\u043e\u0434\u0443\u00bb.<\/p>\n<h3 id=\"toc-9\">\u041b\u0435\u0433\u0430\u043b\u044c\u043d\u043e\u0441\u0442\u044c \u0411\u0435\u0441\u043f\u043b\u0430\u0442\u043d\u044b\u0445 \u0418\u0433\u0440\u043e\u0432\u044b\u0445 \u0410\u0432\u0442\u043e\u043c\u0430\u0442\u043e\u0432<\/h3>\n<p>\u0440\u0430\u0441\u0442\u0435\u0440\u044f\u0442\u044c\u0441\u044f \u0430 \u0440\u0430\u0437\u043d\u043e\u043e\u0431\u0440\u0430\u0437\u0438\u0438 \u0441\u043b\u043e\u0442\u043e\u0432, \u043e\u0437\u043d\u0430\u043a\u043e\u043c\u044c\u0442\u0435\u0441\u044c \u0441 \u043b\u0443\u0447\u0448\u0438\u043c\u0438 \u043f\u0440\u043e\u0438\u0437\u0432\u043e\u0434\u0438\u0442\u0435\u043b\u044f\u043c\u0438. \u0412 \u043f\u0435\u0440\u0432\u043e\u043d\u0430\u0447\u0430\u043b\u044c\u043d\u043e\u043c \u0440\u0435\u0436\u0438\u043c\u0435 \u0432\u0430\u043c \u0431\u0443\u0434\u0443\u0442 \u0432\u044b\u0431\u0440\u0430\u043d\u044b 10 \u0430\u0432\u0442\u043e\u043c\u0430\u0442\u043e\u0432. \u0414\u043b\u044f \u043f\u0435\u0440\u0435\u0445\u043e\u0434\u0430 \u0438\u0437 \u0440\u0435\u0436\u0438\u043c\u044b \u0430\u043a\u0442\u0438\u0432\u043d\u043e\u0439 \u0438\u0433\u0440\u044b \u0432\u044b \u043d\u0435\u043e\u0431\u0445\u043e\u0434\u0438\u043c\u043e \u043d\u0430\u0436\u0430\u0442\u044c \u043a\u043d\u043e\u043f\u043a\u0443 \u00ab\/\u00bb \u043d\u0430 \u043c\u043e\u0435\u0439 \u043a\u043b\u0430\u0432\u0438\u0430\u0442\u0443\u0440\u0435. \u0412 \u0432\u0435\u0440\u0445\u043d\u044f\u044f \u0447\u0430\u0441\u0442\u0438 \u044d\u043a\u0440\u0430\u043d\u0430 \u0441\u0434\u0435\u043b\u0430\u043d\u043e 5 \u0430\u043a\u0442\u0438\u0432\u043d\u044b\u0445 \u0438\u0433\u0440\u043e\u0432\u044b\u0445 \u0430\u0432\u0442\u043e\u043c\u0430\u0442\u043e\u0432, \u0432\u044b\u0431\u0438\u0440\u0430\u0442\u044c \u043a\u043e\u0442\u043e\u0440\u044b\u0435 \u043c\u043e\u0436\u043d\u043e \u043a\u043b\u0430\u0432\u0438\u0448\u0430\u043c\u0438 1, 2, 3, 4, 5 \u2013 \u043d\u0435\u0439\u043d\u044b\u0445 \u0438\u0445 \u043f\u043e\u0437\u0438\u0446\u0438\u0438 \u043d\u0430 \u044d\u043a\u0440\u0430\u043d\u0435.<\/p>\n<ul>\n<li>\u0418\u043d\u043e\u0433\u0434\u0430 \u0432\u0441\u0435\u0433\u043e \u0441\u043b\u043e\u0442 \u0435\u0441\u0442\u044c \u0442\u0440\u0438 \u0438\u043b\u0438 \u043d\u0435\u0441\u043a\u043e\u043b\u044c\u043a\u043e \u0431\u0430\u0440\u0430\u0431\u0430\u043d\u043e\u0432, \u0430 \u043c\u043d\u043e\u0433\u043e \u043b\u0438\u043d\u0438\u0439 \u0440\u0435\u0434\u043a\u043e \u0441\u043e\u0441\u0442\u0430\u0432\u043b\u044f\u043b\u0430 \u0434\u0435\u0432\u044f\u0442\u044c.<\/li>\n<li>\u041d\u043e\u0432\u044b\u0439 \u044d\u0442\u0430\u043f \u0432 \u044d\u0432\u043e\u043b\u044e\u0446\u0438\u0438 \u0430\u0432\u0442\u043e\u043c\u0430\u0442\u043e\u0432 \u043a\u043e\u043d\u0447\u0438\u043b\u0441\u044f \u0432 1976 \u043d\u0435\u0434\u0430\u0432\u043d\u0435\u043c.<\/li>\n<li>\u0410 \u0442\u0430\u043a \u0438 \u0434\u0435\u043c\u043e \u043c\u043e\u0436\u043d\u043e \u043f\u043e\u043d\u0430\u0441\u043b\u044b\u0448\u043a\u0435 \u0438\u0437\u0443\u0447\u0438\u0442\u044c \u0441\u043b\u043e\u0442, \u043e\u043f\u0440\u043e\u0431\u043e\u0432\u0430\u0442\u044c \u0441\u043e \u0441\u0442\u0430\u0432\u043a\u0430\u043c\u0438, \u043c\u0438\u043d\u0438\u043c\u0443\u043c\u043e\u043c \u043b\u0438\u043d\u0438\u0439, \u0430\u0432\u0442\u043e \u0438\u0433\u0440\u043e\u0439 \u0438 \u0434\u0440\u0443\u0433\u0438\u043c\u0438 \u043d\u0430\u0441\u0442\u0440\u043e\u0439\u043a\u0430\u043c\u0438.<\/li>\n<li>\u0418 \u0433\u0435\u0439\u043c\u0435\u0440\u0441\u043a\u0438\u0445 \u0441\u043e\u043e\u0431\u0449\u0435\u0441\u0442\u0432\u0430\u0445, \u0441\u043e\u0446\u0438\u0430\u043b\u044c\u043d\u044b\u0445 \u0441\u0435\u0442\u044f\u0445 \u0438 \u0444\u043e\u0440\u0443\u043c\u0430\u0445 \u043c\u043e\u0436\u043d\u043e \u043d\u0430\u0439\u0442\u0438 \u043d\u0435\u043c\u043d\u043e\u0433\u043e\u0447\u0438\u0441\u043b\u0435\u043d\u043d\u044b\u0445 \u043f\u043e\u043f\u0443\u043b\u044f\u0440\u043d\u044b\u0445 \u043e\u0442\u0437\u044b\u0432\u043e\u0432 \u043e\u0431&#8221; &#8220;\u0434\u0435\u043c\u043e-\u0440\u0435\u0436\u0438\u043c\u0435 \u0438\u0433\u0440\u044b.<\/li>\n<\/ul>\n<p>\u041f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u0442\u0435\u043b\u0438 \u043c\u043e\u0433\u0443\u0442 \u0436\u0435\u043b\u0430\u044e\u0449\u0438\u043c \u0438\u0433\u0440\u0430\u0442\u044c \u0432 \u0440\u0443\u043b\u0435\u0442\u043a\u0443, \u0431\u043b\u044d\u043a\u0434\u0436\u0435\u043a, \u043f\u043e\u043a\u0435\u0440 \u0436\u0435 \u0434\u0440\u0443\u0433\u0438\u0435 \u0438\u0433\u0440\u044b. \u041d\u0438\u043a\u043e\u043c\u0443 \u043d\u0435\u0438\u0437\u0432\u0435\u0441\u0442\u043d\u043e, \u043a\u0430\u043a \u043d\u0430\u0441\u0442\u0440\u043e\u0435\u043d\u044b \u0442\u0430\u043a\u0438\u0435 \u0438\u0433\u0440\u044b \u0436\u0435 \u043f\u043e\u0447\u0435\u043c\u0443 \u0432\u044b\u043f\u0430\u0434\u0430\u044e\u0442 \u043d\u0438\u0445 \u0438\u043b\u0438 \u0438\u043d\u044b\u0435 \u043a\u043e\u043c\u0431\u0438\u043d\u0430\u0446\u0438\u0438. \u0412\u043b\u0430\u0434\u0435\u043b\u044c\u0446\u044b \u0442\u0430\u043a\u0438\u0445 \u043d\u0443\u0436\u0434 \u043c\u043e\u0433\u0443\u0442 \u043f\u043e\u0434\u043a\u0440\u0443\u0447\u0438\u0432\u0430\u0442\u044c \u0440\u0435\u0437\u0443\u043b\u044c\u0442\u0430\u0442\u044b.<\/p>\n<h2 id=\"toc-10\">\u0436\u0435 \u041d\u0430\u0447\u0430\u0442\u044c \u0418\u0433\u0440\u0430\u0442\u044c \u0430 \u0414\u0435\u043c\u043e \u0418\u0433\u0440\u043e\u0432\u044b\u0435 \u0410\u0432\u0442\u043e\u043c\u0430\u0442\u044b<\/h2>\n<p>\u0418\u0433\u0440\u0430\u0442\u044c \u0432 \u043d\u043e\u0432\u0430\u044f \u0438\u0433\u0440\u043e\u0432\u044b\u0435 \u0430\u043f\u043f\u0430\u0440\u0430\u0442\u044b \u043d\u0435\u0436\u0435\u043b\u0430\u044e\u0449\u0438\u043c \u0438 \u0431\u0435\u0437 \u0440\u0435\u0433\u0438\u0441\u0442\u0440\u0430\u0446\u0438\u0438 \u043c\u043e\u0436\u043d\u043e \u043d\u0430 \u043e\u0444\u0438\u0446\u0438\u0430\u043b\u044c\u043d\u043e\u043c \u0441\u0430\u0439\u0442\u0435 \u043f\u0440\u043e\u0432\u0430\u0439\u0434\u0435\u0440\u0430. \u0412\u0430\u0441 \u043d\u0430\u0434\u043e\u0435\u043b\u043e \u0438\u0433\u0440\u0430\u0442\u044c \u0438 \u043e\u0434\u0438\u043d \u0438 \u043e\u043d&#8221; &#8220;\u0430 \u0438\u0433\u0440\u043e\u0432\u043e\u0439 \u0430\u0432\u0442\u043e\u043c\u0430\u0442. \u0421\u0430\u043c\u0438 \u0437\u0430\u0445\u043e\u0442\u0435\u043b\u0438 \u043f\u043e\u043f\u0440\u043e\u0431\u043e\u0432\u0430\u0442\u044c \u043d\u043e\u0432\u043e\u0439 \u0438\u0433\u0440\u043e\u0432\u044b\u0435 \u0430\u0432\u0442\u043e\u043c\u0430\u0442\u044b \u043f\u0438\u043f\u043f\u0430\u0440\u0434\u043e\u043c \u043f\u0440\u0438\u0431\u044b\u043b\u044c\u043d\u044b\u043c\u0438 \u043e\u0442\u0434\u0430\u0447\u0430\u043c\u0438.<\/p>\n<ul>\n<li>\u042d\u0442\u043e\u0442 \u0440\u0435\u0436\u0438\u043c \u0431\u044b\u043b\u043e \u043f\u043e\u043b\u0435\u0437\u0435\u043d \u043d\u043e\u0432\u0438\u0447\u043a\u0430\u043c \u0438 \u0442\u0435\u043c, \u043a\u0442\u043e \u043d\u0435 \u0445\u043e\u0447\u0435\u0442 \u0440\u0438\u0441\u043a\u043e\u0432\u0430\u0442\u044c.<\/li>\n<li>\u041a\u043b\u0438\u0435\u043d\u0442\u044b \u043f\u043e\u043b\u0443\u0447\u0430\u043b\u0438 \u0432\u044b\u043f\u043b\u0430\u0442\u044b, \u0441\u043e\u0431\u0438\u0440\u0430\u044f \u043a\u043e\u043c\u0431\u0438\u043d\u0430\u0446\u0438\u0438&#8221; &#8220;\u0432 \u0431\u0430\u0440\u0430\u0431\u0430\u043d\u0430\u0445 \u0438 \u0443\u043c\u0435\u043d\u044c\u0448\u0438\u0432 \u0441\u0443\u043c\u043c\u044b \u0432 \u0440\u0438\u0441\u043a-\u0438\u0433\u0440\u0435.<\/li>\n<li>\u041a \u0442\u043e\u043c\u0443 \u0436\u0435, \u043e\u043d \u043d\u0435 \u0434\u043e \u043a\u043e\u043d\u0446\u0430 \u043c\u043e\u0436\u0435\u0442 \u043f\u043e\u043d\u0438\u043c\u0430\u0435\u043c \u0435\u0451 \u043f\u0440\u0430\u0432\u0438\u043b\u0430.<\/li>\n<li>\u0415\u0449\u0435 \u0435\u0435 \u043c\u043e\u0436\u0435\u0442 \u0431\u044b\u0442\u044c \u0434\u043e\u0441\u0442\u0443\u043f\u043d\u0430 \u0438 \u0431\u043b\u0430\u0433\u043e\u0434\u0430\u0440\u044f \u043f\u0440\u0438\u0441\u043f\u043e\u0441\u043e\u0431\u043b\u0435\u043d\u043d\u043e\u0439 \u0430\u043a\u0446\u0438\u0438.<\/li>\n<li>\u041d\u0430\u043f\u0440\u0438\u043c\u0435\u0440 \u2013 \u0428\u0432\u0435\u0446\u0438\u044f, \u0414\u0430\u043d\u0438\u044f, \u0420\u0443\u043c\u044b\u043d\u0438\u044f, \u0423\u043a\u0440\u0430\u0438\u043d\u0430, \u0424\u0440\u0430\u043d\u0446\u0438\u044f, \u0418\u0441\u043f\u0430\u043d\u0438\u044f, \u041d\u0438\u0433\u0435\u0440\u0438\u044f \u0438 \u0434\u0440\u0443\u0433\u0438\u0435 \u0441\u0442\u0440\u0430\u043d\u044b.<\/li>\n<\/ul>\n<p>\u0412\u043e\u0441\u0442\u0440\u0435\u0431\u043e\u0432\u0430\u043d\u044b \u0430\u0432\u0442\u043e\u043c\u0430\u0442\u044b Panther Moon, Safari Heat \u0438 \u043c\u043d\u043e\u0433\u0438\u0435 \u043a\u043e\u0442\u043e\u0440\u044b\u0445 \u0440\u0435\u043b\u0438\u0437\u044b \u043f\u0440\u043e\u0438\u0437\u0432\u043e\u0434\u0438\u0442\u0435\u043b\u044f. \u041a\u043e\u043c\u043f\u0430\u043d\u0438\u044f \u0432\u044b\u043f\u0443\u0441\u043a\u0430\u0435\u0442 \u043a\u0430\u0440\u0442\u043e\u0447\u043d\u044b\u0435 \u0438\u0433\u0440\u044b, \u0440\u0443\u043b\u0435\u0442\u043a\u0438, \u043f\u043b\u0430\u0442\u0444\u043e\u0440\u043c\u044b \u0434\u0434\u044f \u0441\u0430\u0439\u0442\u043e\u0432, \u0441\u043e\u0444\u0442 \u0434\u043b\u0438 live-\u043a\u0430\u0437\u0438\u043d\u043e. \u041f\u0440\u0438\u0447\u0435\u043c \u0438\u0433\u0440\u0430\u0442\u044c \u043c\u043e\u0436\u043d\u043e \u0447\u0435\u0440\u0435\u0437 \u0431\u0440\u0430\u0443\u0437\u0435\u0440 \u043f\u043b\u0430\u043d\u0448\u0435\u0442\u0430 \u0438\u043b\u0438 \u0441\u043c\u0430\u0440\u0442\u0444\u043e\u043d\u0430, \u0430 \u0442\u0430\u043a\u0436\u0435 \u0441\u043e \u043f\u043e\u043c\u043e\u0449\u044c\u044e \u0441\u043f\u0435\u0446\u0438\u0430\u043b\u044c\u043d\u043e\u0433\u043e \u043f\u0440\u0438\u043b\u043e\u0436\u0435\u043d\u0438\u044f. \u041d\u0438\u043a\u0430\u043a\u0438\u0445 \u043e\u0433\u0440\u0430\u043d\u0438\u0447\u0435\u043d\u0438\u0439 \u0437\u0434\u0435\u0441\u044c \u043d\u0435\u0442, \u0438 \u0441\u0430\u043c\u0438 \u043f\u043e\u043b\u0443\u0447\u0430\u0435\u0442\u0435 \u0442\u0430\u043a\u0438\u0435 \u0436\u0435 \u0432\u043e\u0437\u043c\u043e\u0436\u043d\u043e\u0441\u0442\u0438 \u0434\u043b\u044f \u0432\u044b\u0438\u0433\u0440\u044b\u0448\u0430 \u0432 \u043a\u0430\u0437\u0438\u043d\u043e.<\/p>\n<h3 id=\"toc-11\">\u041a\u0430\u043a \u041d\u0430\u0447\u0430\u0442\u044c \u0418\u0433\u0440\u0430\u0442\u044c<\/h3>\n<p>\u041e\u0441\u043d\u043e\u0432\u043d\u043e\u0439 \u043d\u0435\u0434\u043e\u0441\u0442\u0430\u0442\u043e\u043a \u0440\u0435\u0436\u0438\u043c\u0430 \u0434\u0435\u043c\u043e \u0432 \u043e\u043d\u043b\u0430\u0439\u043d \u043a\u0430\u0437\u0438\u043d\u043e \u043d\u0438\u043a\u0430\u043a\u0438\u0445 \u0440\u0435\u0433\u0438\u0441\u0442\u0440\u0430\u0446\u0438\u0438 \u2015 \u043e\u0442\u0441\u0443\u0442\u0441\u0442\u0432\u0438\u044f \u043f\u043e\u043b\u0443\u0447\u0438\u0442\u044c \u0440\u0435\u0430\u043b\u044c\u043d\u0443\u044e \u043e\u043f\u043b\u0430\u0442\u0443. \u041a\u043e\u043c\u0443-\u0442\u043e \u0431\u043e\u043b\u044c\u0448\u0435 \u043f\u043e\u0434\u0445\u043e\u0434\u0438\u0442 \u0431\u0435\u0441\u043f\u043b\u0430\u0442\u043d\u044b\u0435 \u0432\u0435\u0440\u0441\u0438\u0438 \u0438\u0433\u0440, \u0442\u0430\u043a \u043a\u0430\u043a \u043c\u043e\u0433\u0443\u0442 \u0438\u0437\u0431\u0435\u0436\u0430\u0442\u044c \u0444\u0438\u043d\u0430\u043d\u0441\u043e\u0432\u044b\u0445 \u0440\u0438\u0441\u043a\u043e\u0432. \u041a\u0442\u043e-\u0442\u043e \u043d\u0430\u043e\u0431\u043e\u0440\u043e\u0442 \u0438\u0433\u0440\u0430\u0435\u0442 \u0432 \u0430\u043f\u043f\u0430\u0440\u0430\u0442\u044b \u0442\u043e\u043b\u044c\u043a\u043e \u043d\u0430 \u0434\u0435\u043d\u044c\u0433\u0438, \u044d\u0442\u043e\u0433\u043e \u0441\u043f\u043e\u043b\u043d\u0430 \u043e\u0449\u0443\u0442\u0438\u0442\u044c \u0441\u043e\u0431\u0441\u0442\u0432\u0435\u043d\u043d\u044b\u0439 \u0430\u0437\u0430\u0440\u0442.<\/p>\n<ul>\n<li>\u041a\u0430\u0436\u0434\u044b\u0439 \u0440\u0430\u0443\u043d\u0434 \u043c\u043e\u0436\u0435\u0442 \u0432\u043a\u043b\u044e\u0447\u0430\u0442\u044c \u0432 \u0441\u0435\u0431\u044f \u043d\u0435\u0441\u043a\u043e\u043b\u044c\u043a\u043e \u0441\u043f\u0438\u043d\u043e\u0432 \u0441 \u0440\u0430\u0441\u0442\u0443\u0449\u0438\u043c \u043c\u043d\u043e\u0436\u0438\u0442\u0435\u043b\u0435\u043c.<\/li>\n<li>\u0418 \u0431\u043e\u043b\u044c\u0448\u0438\u043d\u0441\u0442\u0432\u0435 \u043e\u043d\u043b\u0430\u0439\u043d-\u043a\u0430\u0437\u0438\u043d\u043e \u043f\u0440\u0435\u0434\u0443\u0441\u043c\u043e\u0442\u0440\u0435\u043d\u0430 \u0442\u0430\u043a\u0430\u044f \u043f\u0440\u0430\u043a\u0442\u0438\u043a\u0430.<\/li>\n<li>\u0416\u0435 \u043e\u0442\u0434\u0435\u043b\u044c\u043d\u044b\u0435 \u0438\u0433\u0440\u043e\u0432\u044b\u0435 \u043a\u043b\u0443\u0431\u044b \u0434\u0430\u0436\u0435 \u0434\u043b\u044f \u0434\u0435\u043c\u043e \u043f\u0440\u043e\u0441\u044f\u0442 \u043f\u0440\u043e\u0439\u0442\u0438 \u0440\u0435\u0433\u0438\u0441\u0442\u0440\u0430\u0446\u0438\u044e.<\/li>\n<\/ul>\n<p>\u041a\u0430\u043a \u0434\u043e\u043f\u043e\u043b\u043d\u0438\u0442\u0435\u043b\u044c\u043d\u044b\u0439 \u0440\u0430\u0443\u043d\u0434, \u0442\u043e\u0442 \u0430\u043a\u0442\u0438\u0432\u0438\u0440\u0443\u0435\u0442\u0441\u044f \u043f\u043e\u0441\u043b\u0435 \u0441\u043e\u0441\u0442\u0430\u0432\u0438\u0442\u044c \u0432\u044b\u0438\u0433\u0440\u044b\u0448\u043d\u043e\u0439 \u043a\u043e\u043c\u0431\u0438\u043d\u0430\u0446\u0438\u0438. \u0410 \u0440\u0438\u0441\u043a-\u0438\u0433\u0440\u0435 \u043c\u043e\u0436\u043d\u043e \u0443\u0434\u0432\u043e\u0438\u0442\u044c \u0432\u044b\u0438\u0433\u0440\u044b\u0448 \u0438\u043b\u0438 \u043f\u043e\u0442\u0435\u0440\u044f\u0435\u0442\u0435 \u0432\u0441\u0435. \u0423\u043a\u0430\u0437\u0430\u043d\u043d\u044b\u0435 \u0442\u0435\u0440\u043c\u0438\u043d\u044b \u0432 \u043e\u0441\u043d\u043e\u0432\u043d\u043e\u043c \u043a\u0430\u0441\u0430\u0435\u0442\u0441\u044f \u0441\u043b\u043e\u0442\u043e\u0432, \u043d\u043e \u043d\u0435\u043a\u043e\u0442\u043e\u0440\u044b\u0435 \u043f\u043e\u043d\u044f\u0442\u0438\u044f \u0431\u0443\u0434\u0443\u0442 \u0430\u043a\u0442\u0443\u0430\u043b\u044c\u043d\u044b \u0438 \u0434\u043b\u044f \u043a\u0430\u0440\u0442\u043e\u0447\u043d\u044b\u0445 \u0438\u0433\u0440. \u041e\u0437\u043d\u0430\u043a\u043e\u043c\u0438\u0442\u044c\u0441\u044f \u0441 \u043d\u0438\u043c\u0438 \u0431\u0443\u0434\u0435\u0442 \u0436\u0435\u043b\u0430\u0442\u0435\u043b\u044c\u043d\u043e \u043a\u0430\u043a \u043d\u043e\u0432\u0438\u0447\u043a\u0430\u043c, \u0430 \u0438 \u043e\u043f\u044b\u0442\u043d\u044b\u043c \u0438\u0433\u0440\u043e\u043a\u0430\u043c.<\/p>\n<h3 id=\"toc-12\">\u0436\u0435 \u041b\u0443\u0447\u0448\u0438\u0435 \u0421\u043b\u043e\u0442\u044b \u0434\u043b\u0438 \u041d\u0430\u0448\u0438\u0445 \u0418\u0433\u0440\u043e\u043a\u043e\u0432!<\/h3>\n<p>\u0422\u0435 \u043d\u0430\u043f\u0440\u0438\u043c\u0435\u0440 \u0438\u043d\u044b\u0435 \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f, \u0443\u043a\u0430\u0437\u0430\u043d\u043d\u044b\u0435 \u043d\u0430 \u0431\u0430\u0440\u0430\u0431\u0430\u043d\u0435 \u0441\u043b\u043e\u0442\u0430. \u0418\u0441\u043f\u043e\u043b\u044c\u0437\u0443\u044e\u0442\u0441\u044f \u043f\u0440\u0438 \u0441\u043e\u0441\u0442\u0430\u0432\u0438\u0442\u044c \u0432\u044b\u0438\u0433\u0440\u044b\u0448\u043d\u044b\u0445 \u043a\u043e\u043c\u0431\u0438\u043d\u0430\u0446\u0438\u0439. \u0410 \u043d\u0438\u0445 \u0432\u044b\u0438\u0433\u0440\u044b\u0448 \u0444\u043e\u0440\u043c\u0438\u0440\u0443\u0435\u0442\u0441\u044f \u043d\u0435 \u043f\u043e \u043b\u0438\u043d\u0438\u044f\u043c, \u0430 \u043f\u043e \u043c\u043d\u043e\u0433\u0438\u043c \u043a\u043e\u043c\u0431\u0438\u043d\u0430\u0446\u0438\u044f\u043c \u0441\u0438\u043c\u0432\u043e\u043b\u043e\u0432.<\/p>\n<ul>\n<li>\u0422\u0430\u043a\u0436\u0435 \u0434\u043e\u0441\u0442\u0443\u043f\u0435\u043d \u0445\u043e\u0440\u043e\u0448\u0438\u0439 \u0432\u044b\u0431\u043e\u0440 \u0434\u0438\u0441\u0446\u0438\u043f\u043b\u0438\u043d \u0434\u043b\u044f \u0441\u0442\u0430\u0432\u043e\u043a \u043d\u0430 \u0441\u043f\u043e\u0440\u0442.<\/li>\n<li>\u0412 \u043f\u043e\u0441\u043b\u0435\u0434\u043d\u0438\u0435 \u043d\u0435\u043c\u043d\u043e\u0433\u043e \u043b\u0435\u0442 \u043a\u043e\u043c\u043f\u0430\u043d\u0438\u0438 \u0441\u043c\u043e\u0433 \u0441\u0434\u0435\u043b\u0430\u0442\u044c \u043a\u0430\u0447\u0435\u0441\u0442\u0432\u0435\u043d\u043d\u044b\u0439 \u043f\u0435\u0440\u0435\u0445\u043e\u0434 \u0432\u043f\u0435\u0440\u0435\u0434.<\/li>\n<li>\u041f\u0435\u0440\u0435\u0434 \u0442\u0435\u043c, \u043a\u0430\u043a \u0438\u0433\u0440\u0430\u0442\u044c \u0432 \u0431\u0435\u0441\u043f\u043b\u0430\u0442\u043d\u044b\u0435 \u0438\u0433\u0440\u043e\u0432\u044b\u0435 \u0430\u0432\u0442\u043e\u043c\u0430\u0442\u044b, \u0432\u044b \u043d\u0443\u0436\u043d\u043e \u043f\u043e\u0437\u043d\u0430\u043a\u043e\u043c\u0438\u0442\u044c\u0441\u044f \u0441 \u043b\u0438\u043d\u0438\u044f\u043c\u0438 \u0432\u044b\u043f\u043b\u0430\u0442.<\/li>\n<li>\u0421\u0435\u0440\u0442\u0438\u0444\u0438\u043a\u0430\u0442\u044b \u043f\u043e\u0434\u0442\u0432\u0435\u0440\u0436\u0434\u0430\u044e\u0442, \u0447\u0442\u043e \u043e\u043f\u0435\u0440\u0430\u0442\u043e\u0440 \u0440\u0430\u0437\u043c\u0435\u0449\u0430\u0435\u0442 \u043e\u0440\u0438\u0433\u0438\u043d\u0430\u043b\u044c\u043d\u044b\u0439 \u0441\u043e\u0444\u0442 \u0438 \u043d\u0435 \u0432\u043c\u0435\u0448\u0438\u0432\u0430\u0435\u0442\u0441\u044f \u0432 \u0440\u0435\u0437\u0443\u043b\u044c\u0442\u0430\u0442\u044b \u0432\u0440\u0430\u0449\u0435\u043d\u0438\u0439.<\/li>\n<\/ul>\n<p>\u041e\u0431\u044b\u0447\u043d\u043e \u0431\u043e\u043d\u0443\u0441\u043d\u0430\u044f \u0438\u0433\u0440\u0430 \u043e\u0431\u0445\u043e\u0434\u0438\u0442\u0441\u044f \u043d\u0435\u0434\u0435\u0448\u0435\u0432\u043e, \u043d\u043e \u044d\u0442\u043e \u043f\u043e\u0437\u0432\u043e\u043b\u044f\u0435\u0442 \u0432\u0430\u043c \u0441\u0440\u0430\u0437\u0443 \u0430 \u0434\u043e\u0439\u0442\u0438 \u0434\u043e \u043e\u0441\u043e\u0431\u0435\u043d\u043d\u043e\u0433\u043e \u0440\u0430\u0443\u043d\u0434\u0430. \u041a\u043e\u043b\u0438\u0447\u0435\u0441\u0442\u0432\u043e \u0438\u0433\u0440 \u0441 \u0442\u0430\u043a\u043e\u0439 \u0444\u0443\u043d\u043a\u0446\u0438\u0435\u0439 \u043f\u0440\u043e\u0434\u043e\u043b\u0436\u0430\u0435\u0442 \u0443\u0432\u0435\u043b\u0438\u0447\u0438\u0432\u0430\u0442\u044c\u0441\u044f. \u0416\u0435 \u0447\u0442\u043e \u0432\u044b \u0442\u043e \u043d\u0430\u0439\u0434\u0435\u0442\u0435 \u0432\u0430\u0440\u0438\u0430\u043d\u0442 \u0441\u043e Feature Buy \u0432 \u0430\u0441\u0441\u043e\u0440\u0442\u0438\u043c\u0435\u043d\u0442\u0435 \u0441\u0432\u043e\u0435\u0433\u043e \u043c\u043e\u0435\u0433\u043e \u043a\u0430\u0437\u0438\u043d\u043e.<\/p>\n<h2 id=\"toc-13\">\u0421\u043a\u0430\u0447\u0430\u0442\u044c \u0418\u0433\u0440\u043e\u0432\u044b\u0435 \u0410\u0432\u0442\u043e\u043c\u0430\u0442\u044b \u0438 \u0410\u043d\u0434\u0440\u043e\u0438\u0434 \u1408 \u041c\u043e\u0431\u0438\u043b\u044c\u043d\u043e\u0435 \u041f\u0440\u0438\u043b\u043e\u0436\u0435\u043d\u0438\u0435 \u041d\u0430 \u0422\u0435\u043b\u0435\u0444\u043e\u043d<\/h2>\n<p>\u041e\u0431\u044b\u0447\u043d\u043e \u043e\u0442\u0447\u0435\u0442 \u0438\u0434\u0435\u0442 \u0441\u043b\u0435\u0432\u0430 \u043d\u0430\u043f\u0440\u0430\u0432\u043e, \u0430 \u0440\u0435\u0434\u043a\u0438\u0445 \u0441\u043b\u0443\u0447\u0430\u044f\u0445 \u0442\u0430\u043a \u043f\u0440\u043e\u0438\u0441\u0445\u043e\u0434\u0438\u0442 \u0432 \u043e\u0431\u0440\u0430\u0442\u043d\u043e\u043c \u043d\u0430\u043f\u0440\u0430\u0432\u043b\u0435\u043d\u0438\u0438. \u041a \u0432\u044b\u0438\u0433\u0440\u044b\u0448\u0443 \u043f\u0440\u0438\u0432\u043e\u0434\u044f\u0442 \u043e\u043f\u0440\u0435\u0434\u0435\u043b\u0435\u043d\u043d\u044b\u0435 \u043a\u043e\u043c\u0431\u0438\u043d\u0430\u0446\u0438\u0438 \u0441\u0438\u043c\u0432\u043e\u043b\u043e\u0432. \u0418\u0433\u0440\u043e\u043a\u0430\u043c \u043d\u0435\u043e\u0431\u0445\u043e\u0434\u0438\u043c\u043e \u0441\u0442\u0440\u0435\u043c\u0438\u0442\u044c\u0441\u044f \u043a \u043f\u043e\u043b\u0443\u0447\u0435\u043d\u0438\u044e \u043d\u0443\u0436\u043d\u043e\u0433\u043e \u0440\u0435\u0437\u0443\u043b\u044c\u0442\u0430\u0442\u0430. \u0418\u043d\u043e\u0433\u0434\u0430 \u0431\u0435\u0437\u0434\u0435\u043f\u044b \u0432\u043a\u043b\u044e\u0447\u0430\u0435\u0442 \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0430 \u043b\u043e\u044f\u043b\u044c\u043d\u043e\u0441\u0442\u0438 \u043e\u043d\u043b\u0430\u0439\u043d \u043a\u0430\u0437\u0438\u043d\u043e \u0438\u043b\u0438 \u0434\u043e\u043f\u043e\u043b\u043d\u0438\u0442\u0435\u043b\u044c\u043d\u044b\u0435 \u0430\u043a\u0446\u0438\u0438 \u043e\u0442 \u043e\u043f\u0435\u0440\u0430\u0442\u043e\u0440\u043e\u0432 \u043a\u043b\u0443\u0431\u0430 (\u043d\u0430\u043f\u0440\u0438\u043c\u0435\u0440, \u043c\u0430\u0440\u0430\u0444\u043e\u043d \u0438\u043b\u0438 \u043a\u0432\u0435\u0441\u0442).<\/p>\n<p>\u041d\u0435 \u0443\u0436\u0435 \u043d\u0435 \u043f\u043e\u0441\u043b\u0435\u0434\u043d\u0438\u0439 \u0432\u044b\u0438\u0433\u0440\u044b\u0432\u0430\u043b \u0437\u0434\u0435\u0441\u044c \u043a\u0440\u0443\u043f\u043d\u044b\u0435 \u0441\u0443\u043c\u043c\u044b \u0438 \u0440\u0430\u0437 \u0440\u0430\u0437 \u0432\u043e\u0437\u0432\u0440\u0430\u0449\u0430\u044e\u0441\u044c \u0438 \u043d\u043e\u0432\u044b\u043c\u0438 \u044d\u043c\u043e\u0446\u0438\u044f\u043c\u0438 \u0430 \u0432\u043e\u0437\u043c\u043e\u0436\u043d\u043e\u0441\u0442\u044f\u043c\u0438. \u042d\u0442\u043e \u0442\u043e\u043b\u044c\u043a\u043e \u043e\u0434\u043d\u0430 \u043d\u0435\u0443\u0432\u044f\u0434\u0430\u0435\u043c\u0430\u044f \u043a\u043b\u0430\u0441\u0441\u0438\u043a\u0430, \u043f\u043e\u0433\u0440\u0443\u0436\u0430\u044e\u0449\u0430\u044f \u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u0442\u0435\u043b\u044f \u0432 \u043e\u0441\u043e\u0431\u044b\u0439 \u043c\u0438\u0440. \u041e\u043a\u0443\u043d\u0443\u0432\u0448\u0438\u0441\u044c \u0432 \u0430\u0442\u043c\u043e\u0441\u0444\u0435\u0440\u0443 \u0422\u044b\u0441\u044f\u0447\u0435\u043b\u0435\u0442\u043d\u044e\u044e \u0415\u0433\u0438\u043f\u0442\u0430, \u0432\u044b \u0432\u044b\u0441\u0442\u0443\u043f\u0430\u0435\u0442\u0435 \u0432 \u0440\u043e\u043b\u0438 \u0438\u0441\u0441\u043b\u0435\u0434\u043e\u0432\u0430\u0442\u0435\u043b\u044f \u0438 \u043e\u0442\u043f\u0440\u0430\u0432\u043b\u044f\u0435\u0442\u0435\u0441\u044c \u0438 \u043f\u043e\u0438\u0441\u043a\u0438 \u041a\u043d\u0438\u0433\u0438 \u0421\u043c\u0435\u0440\u0442\u0438. \u0418\u0433\u0440\u0430 \u0434\u043e\u043f\u043e\u043b\u043d\u044f\u0435\u0442\u0441\u044f \u0434\u0440\u0443\u0433\u0438\u043c\u0438 \u0444\u0443\u043d\u043a\u0446\u0438\u044f\u043c\u0438, \u0431\u043b\u0430\u0433\u043e\u0434\u0430\u0440\u044f \u043d\u0438\u043c\u0438 \u043c\u043e\u0436\u043d\u043e \u0443\u0432\u0435\u043b\u0438\u0447\u0438\u0442\u044c \u0440\u0430\u0437\u043c\u0435\u0440 \u0432\u044b\u0438\u0433\u0440\u044b\u0448\u0430. \u0412\u044b\u0438\u0433\u0440\u044b\u0448 \u0430 \u0430\u0432\u0442\u043e\u043c\u0430\u0442\u0435 \u043e\u043f\u0440\u0435\u0434\u0435\u043b\u044f\u0435\u0442\u0441\u044f \u043f\u0440\u043e\u0442\u0438\u0432\u043e\u0440\u0435\u0447\u0438\u043b\u043e \u0434\u0435\u0439\u0441\u0442\u0432\u0443\u044e\u0449\u0435\u043c\u0443 \u0430\u043b\u0433\u043e\u0440\u0438\u0442\u043c\u0443. \u041e\u043d\u0438 \u0432\u044b\u043f\u0430\u0434\u0430\u044e\u0449\u0438\u0435 \u0441\u0438\u043c\u0432\u043e\u043b\u044b \u043e\u0431\u0445\u043e\u0434\u044f\u0442\u0441\u044f&#8221; &#8220;\u043d\u0438\u0445, \u0438 \u044d\u0442\u0438 \u0438\u043c\u0435\u0432\u0448\u0435\u0435 \u0440\u0430\u0441\u043f\u0440\u0435\u0434\u0435\u043b\u044f\u044e\u0442\u0441\u044f \u043f\u043e \u0440\u0430\u0437\u043d\u044b\u043c \u043b\u0438\u043d\u0438\u044f\u043c \u0438 \u0431\u0430\u0440\u0430\u0431\u0430\u043d\u0430\u043c.<\/p>\n","protected":false},"excerpt":{"rendered":"<p>&#8220;\u0434\u0435\u043c\u043e \u0418\u0433\u0440\u043e\u0432\u044b\u0435 \u0410\u0432\u0442\u043e\u043c\u0430\u0442\u044