0430\u0440\u0442\u043d\u044b\u0445 \u0440\u0430\u0437\u0432\u043b\u0435\u0447\u0435\u043d\u0438\u0439 \u043d\u043e\u0432\u043e\u0433\u043e \u043f\u043e\u043a\u043e\u043b\u0435\u043d\u0438\u044f \u043f\u0440\u043e\u0432\u0430\u0439\u0434\u0435\u0440\u044b \u0442\u043e\u043b\u044c\u043a\u043e \u043e\u0433\u0440\u0430\u043d\u0438\u0447\u0438\u0432\u0430\u044e\u0442 \u0441\u0435\u0431\u044f \u043f\u0440\u0438\u0432\u044b\u0447\u043d\u043e\u0439 \u043c\u043e\u0434\u0435\u043b\u044c\u044e \u0441 \u0431\u0430\u0440\u0430\u0431\u0430\u043d\u0430\u043c\u0438 \u0438 \u043b\u0438\u043d\u0438\u044f\u043c\u0438.<\/p>\n<h2 id=\"toc-0\">\u0411\u0435\u0441\u043f\u043b\u0430\u0442\u043d\u0430\u044f \u0412\u0435\u0440\u0441\u0438\u044f<\/h2>\n<p>\u041d\u0435\u043c\u0443 \u043d\u0435\u0434\u043e\u0441\u0442\u0430\u0442\u043a\u0430\u043c \u043e\u0442\u043d\u043e\u0441\u0438\u0442\u0441\u044f \u0430, \u0447\u0442\u043e \u0432 \u0434\u0435\u043c\u043e\u0440\u0435\u0436\u0438\u043c\u0435 \u043d\u0435\u0432\u043e\u0437\u043c\u043e\u0436\u043d\u043e \u043f\u043e\u043b\u0443\u0447\u0438\u0442\u044c \u0440\u0435\u0430\u043b\u044c\u043d\u044b\u0445 \u0432\u044b\u043f\u043b\u0430\u0442\u0443. \u0422\u0430\u043a\u0436\u0435 \u0432 \u0431\u0435\u0441\u043f\u043b\u0430\u0442\u043d\u044b\u0445 \u0430\u043f\u043f\u0430\u0440\u0430\u0442\u0430\u0445 \u043d\u0438\u043a\u0430\u043a \u043e\u0442\u044b\u0433\u0440\u044b\u0432\u0430\u0442\u044c \u0431\u043e\u043d\u0443\u0441\u044b. \u0414\u0434\u044f \u0434\u043e\u0441\u0442\u0443\u043f\u0430 \u043a \u043f\u043e\u043b\u043d\u0435\u0439\u0448\u0435\u043c\u0443 \u0444\u0443\u043d\u043a\u0446\u0438\u043e\u043d\u0430\u043b\u0443 \u043a\u0430\u0437\u0438\u043d\u043e \u0432\u0430\u0448\u0430 \u0440\u0435\u0433\u0438\u0441\u0442\u0440\u0430\u0446\u0438\u044f \u0438 \u0434\u0435\u043f\u043e\u0437\u0438\u0442. \u041a\u0430\u0436\u0434\u0430\u044f \u0440\u0430\u0437\u043d\u043e\u0432\u0438\u0434\u043d\u043e\u0441\u0442\u044c \u0441\u0443\u0449\u0435\u0441\u0442\u0432\u0443\u0435\u0442 \u043e\u043f\u0440\u0435\u0434\u0435\u043b\u0435\u043d\u043d\u044b\u0435 \u043e\u0441\u043e\u0431\u0435\u043d\u043d\u043e\u0441\u0442\u0438. \u0422\u0430\u043a\u0436\u0435, \u0432 \u043a\u043b\u0430\u0441\u0441\u0438\u0447\u0435\u0441\u043a\u0438\u0445 \u0438\u0433\u0440\u0430\u0445 \u043a\u043e\u043b\u0438\u0447\u0435\u0441\u0442\u0432\u043e \u0430\u043a\u0442\u0438\u0432\u043d\u044b\u0445 \u043b\u0438\u043d\u0438\u0439 \u0444\u0438\u043a\u0441\u0438\u0440\u043e\u0432\u0430\u043d\u043e, \u0430 \u0430&#8221; &#8220;3D \u0441\u043b\u043e\u0442\u0430\u0445 \u0438\u0433\u0440\u043e\u0432\u043e\u0439 \u044d\u0442\u0430\u043f\u0430 \u0441\u043e\u043f\u0440\u043e\u0432\u043e\u0436\u0434\u0430\u044e\u0442 \u0430\u043d\u0438\u043c\u0430\u0446\u0438\u043e\u043d\u043d\u044b\u0435 \u044d\u0444\u0444\u0435\u043a\u0442\u044b.<\/p>\n<p>\u0414\u0435\u043b\u0430\u044f \u0441\u0442\u0430\u0432\u043a\u0438 \u043e\u043d\u043b\u0430\u0439\u043d \u0432 \u0442\u0430\u043a\u0438\u0445 \u0430\u0437\u0430\u0440\u0442\u043d\u044b\u0445 \u0438\u0433\u0440\u0430\u0445 \u043f\u0440\u043e\u0432\u0430\u0439\u0434\u0435\u0440\u0430, \u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u0442\u0435\u043b\u044c \u043a\u043e\u043f\u0438\u0442 \u0432\u0438\u0440\u0442\u0443\u0430\u043b\u044c\u043d\u0443\u044e \u0432\u0430\u043b\u044e\u0442\u0443, \u043a\u043e\u0442\u043e\u0440\u0443\u044e \u043c\u043e\u0436\u043d\u043e \u043e\u0431\u043c\u0435\u043d\u044f\u0442\u044c \u043d\u0430 \u0431\u043e\u043d\u0443\u0441\u044b, \u0444\u0440\u0438\u0441\u043f\u0438\u043d\u044b \u0438\u043b\u0438 \u043c\u0433\u043d\u043e\u0432\u0435\u043d\u043d\u044b\u0435 \u0432\u044b\u043f\u043b\u0430\u0442\u044b. \u041d\u043e\u0432\u0438\u0447\u043a\u0430\u043c, \u043a\u043e\u0442\u043e\u0440\u044b\u0435 \u043f\u043b\u0430\u043d\u0438\u0440\u0443\u044e\u0442 \u0438\u0433\u0440\u0430\u0442\u044c \u043e\u043d\u043b\u0430\u0439\u043d \u0432 \u0438\u0433\u0440\u043e\u0432\u044b\u0435 \u0430\u0432\u0442\u043e\u043c\u0430\u0442\u044b \u0432 \u043a\u0430\u0437\u0438\u043d\u043e \u0441\u043e \u0434\u0435\u043c\u043e \u0441\u0447\u0435\u0442\u043e\u043c, \u0447\u0435\u043c \u0432\u0441\u0435\u0433\u043e \u043d\u0430\u0447\u0430\u0442\u044c \u043f\u0438\u043f\u043f\u0430\u0440\u0434\u043e\u043c \u0441\u0430\u043c\u044b\u0445 \u0438\u0437\u0432\u0435\u0441\u0442\u043d\u044b\u0445 \u0440\u0430\u0431\u043e\u0442. \u0422\u043e\u043b\u044c\u043a\u043e \u043b\u0438\u0446\u0435\u043d\u0437\u0438\u043e\u043d\u043d\u043e\u0435 \u0434\u0435\u043c\u043e \u0433\u0430\u0440\u0430\u043d\u0442\u0438\u0440\u0443\u0435\u0442 \u043f\u043e\u043b\u043d\u043e\u0435 \u0441\u043b\u0443\u0447\u0430\u0439\u043d\u043e\u0441\u0442\u0438 \u0445\u0430\u0440\u0430\u043a\u0442\u0435\u0440\u0438\u0441\u0442\u0438\u043a, \u043f\u0440\u0438\u0437\u043e\u0432\u044b\u0445 \u0444\u0443\u043d\u043a\u0446\u0438\u0439 \u0438 \u043d\u0430\u0441\u0442\u0440\u043e\u0435\u043a, \u0447\u0430\u0441\u0442\u043e\u0442\u044b \u0432\u044b\u0438\u0433\u0440\u044b\u0448\u0435\u0439 \u0438 \u0434\u0440\u0443\u0433\u0438\u0445 \u043f\u0430\u0440\u0430\u043c\u0435\u0442\u0440\u043e\u0432, \u0437\u0430\u043b\u043e\u0436\u0435\u043d\u043d\u044b\u0445 \u043f\u0440\u043e\u0438\u0437\u0432\u043e\u0434\u0438\u0442\u0435\u043b\u0435\u043c <a href=\"https:\/\/kp-journal.ru\/\">1\u0445\u0431\u0435\u0442<\/a>.<\/p>\n<h3 id=\"toc-1\">\u041a\u0440\u0438\u0442\u0435\u0440\u0438\u0438 \u0412\u044b\u0431\u043e\u0440\u0430 \u0421\u0430\u0439\u0442\u0430 \u043f\u0438\u043f\u043f\u0430\u0440\u0434\u043e\u043c \u0411\u0435\u0441\u043f\u043b\u0430\u0442\u043d\u044b\u043c\u0438 \u0418\u0433\u0440\u043e\u0432\u044b\u043c\u0438 \u043d\u0430\u043f\u0435\u0440\u0435\u0432\u0435\u0441<\/h3>\n<p>\u0421\u0443\u0449\u0435\u0441\u0442\u0432\u0443\u0435\u0442 2 \u043f\u0440\u0435\u0436\u043d\u0438\u0445 \u043a\u043b\u0438\u0435\u043d\u0442\u0430 \u2013 \u0432\u0435\u0440\u0441\u0438\u044f \u0434\u043b\u044f Windows \u0430 mac OS. \u0427\u0435\u0440\u0435\u0441\u0447\u0443\u0440 \u0441\u043b\u0435\u0434\u043e\u0432\u0430\u0442\u044c \u043f\u043e\u0448\u0430\u0433\u043e\u0432\u043e\u0439 \u0438\u043d\u0441\u0442\u0440\u0443\u043a\u0446\u0438\u0438 \u0434\u043e \u0437\u0430\u0432\u0435\u0440\u0448\u0435\u043d\u0438\u044f \u043f\u0440\u043e\u0446\u0435\u0441\u0441\u0430 \u0438\u043d\u0441\u0442\u0430\u043b\u043b\u044f\u0446\u0438\u0438. \u041e\u0431\u044f\u0437\u0430\u0442\u0435\u043b\u044c\u043d\u043e \u0443\u0441\u0442\u0430\u043d\u043e\u0432\u0438\u043c \u044f\u0440\u043b\u044b\u043a \u043d\u0430 \u043f\u043e\u0447\u0435\u0442\u043d\u043e\u043c \u0441\u0442\u043e\u043b\u0435 \u0434\u043b\u044f \u0431\u044b\u0441\u0442\u0440\u043e\u0433\u043e \u043f\u043e\u0438\u0441\u043a\u0430 \u043a\u0430\u0437\u0438\u043d\u043e. \u041d\u043e\u0432\u0430\u044f \u0444\u0443\u043d\u043a\u0446\u0438\u0438 \u0432 \u0441\u043e\u0432\u0440\u0435\u043c\u0435\u043d\u043d\u044b\u0445 \u0441\u043b\u043e\u0442\u0430\u0445 \u043c\u043e\u0433\u0443\u0442 \u043f\u043e\u043a\u0430\u0437\u0430\u0442\u044c\u0441\u044f \u0441\u0442\u0440\u0430\u043d\u043d\u044b\u043c\u0438 \u0442\u0435\u043c, \u043a\u0442\u043e \u043f\u0440\u0438\u0432\u044b\u043a \u043a \u043d\u0430\u0437\u044b\u0432\u0430\u0435\u043c\u043e\u043c\u0443 \u0433\u0435\u0439\u043c\u043f\u043b\u0435\u044e. \u0414\u0435\u043c\u043e\u0432\u0435\u0440\u0441\u0438\u044f \u2014 \u044d\u0442\u043e \u043e\u0442\u043b\u0438\u0447\u043d\u044b\u0439 \u043c\u0435\u0442\u043e\u0434\u043e\u043c \u043e\u0441\u0432\u043e\u0438\u0442\u044c \u0438\u043d\u043d\u043e\u0432\u0430\u0446\u0438\u043e\u043d\u043d\u044b\u0435 \u0440\u0435\u0448\u0438\u0442\u044c \u043f\u0440\u043e\u0432\u0430\u0439\u0434\u0435\u0440\u043e\u0432 \u0431\u0435\u0437 \u0440\u0435\u0433\u0438\u0441\u0442\u0440\u0430\u0446\u0438\u0438 \u0438 \u0437\u0430\u0442\u0440\u0430\u0442. \u041d\u0435\u043c\u043d\u043e\u0433\u0438\u0445 \u043d\u043e\u0432\u0438\u043d\u043e\u043a \u0441 \u043f\u0435\u0440\u0432\u044b\u0445 \u0434\u043d\u0435\u0439 \u0432\u